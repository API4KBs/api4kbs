  </owl:Class>

  <!-- Script Output -->
  

  <owl:Class rdf:about="http://www.omg.org/spec/API4KB/API4KBTerminology/QueryingEvent">
    <rdfs:label>querying event</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">action event that applies a query to a focused OMS resource producing a collection of answer
      substitutions.</skos:definition>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/ActionEvent"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/hasFirstInput"/>
        <owl:onClass rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/QueryResource"/>
        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/hasSecondInput"/>
        <owl:onClass rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/FocusedOMSResource"/>
        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/hasOutput"/>
        <owl:onClass rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/AnswerSubstitutionCollection"/>
        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
