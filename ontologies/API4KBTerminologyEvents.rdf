  <!-- Operation Eventw -->
  <owl:Class rdf:about="http://www.omg.org/spec/API4KB/API4KBTerminology/OperationEvent">
    <rdfs:label>operation event</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">event of an API4KB Operation.</skos:definition>
    <skos:example>Example: the adding of an Asset to an immutable KB to generate a new immutable KB,
      executed by some application. </skos:example>
    <owl:subClassOf>
        <owl:Restriction>
          <owl:onProperty
            rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/hasOutput"/>
          <owl:onClass
            rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/KnowledgeSource"/>
          <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger"
            >1</owl:qualifiedCardinality>
        </owl:Restriction>
    </owl:subClassOf>
    <owl:equivalentClass> 
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description
            rdf:about="http://www.omg.org/spec/API4KB/API4KBTerminology/Event"/>
          <owl:Restriction>
            <owl:onProperty
              rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/isEventof"/>
            <owl:someValuesFrom
              rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/Operation"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <owl:disjointUnionOf rdf:parseType="Collection">
      <rdf:Description
        rdf:about="http://www.omg.org/spec/API4KB/API4KBTerminology/InternalOperationEvent"/>
      <rdf:Description
        rdf:about="http://www.omg.org/spec/API4KB/API4KBTerminology/PublicOperationEvent"/>
    </owl:disjointUnionOf>
  </owl:Class>

  <!-- Internal Operations Events -->

  <owl:Class rdf:about="http://www.omg.org/spec/API4KB/API4KBTerminology/InternalOperationEvent">
    <rdfs:label>internal operation event</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">event of an Internal Operation.</skos:definition>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description
            rdf:about="http://www.omg.org/spec/API4KB/API4KBTerminology/OperationEvent"/>
          <owl:Restriction>
            <owl:onProperty
              rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/isEventof"/>
            <owl:someValuesFrom
              rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/InternalOperation"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <owl:disjointWith
      rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/PublicOperationEvent"/>
  </owl:Class>
  
  <owl:Class rdf:about="http://www.omg.org/spec/API4KB/API4KBTerminology/ActionEvent">
    <rdfs:label>action event</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">event of an Action.</skos:definition>
    <owl:equivalentClass> 
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description
            rdf:about="http://www.omg.org/spec/API4KB/API4KBTerminology/InternalOperationEvent"/>
          <owl:Restriction>
            <owl:onProperty
              rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/isEventof"/>
            <owl:someValuesFrom
              rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/Action"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>


  <!-- http://www.omg.org/spec/API4KB/API4KBTerminology/InStreaming -->

  <owl:Class rdf:about="http://www.omg.org/spec/API4KB/API4KBTerminology/InStreaming">
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/LiftingEvent"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/hasInput"/>
        <owl:someValuesFrom
          rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/KnowledgeItem"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/hasOutput"/>
        <owl:someValuesFrom
          rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/KnowledgeEncoding"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <skos:definition rdf:datatype="&xsd;string">Action Event of transforming a knowledge item into a
      knowledge encoding.</skos:definition>
    <rdfs:isDefinedBy>http://www.omg.org/spec/API4KB/API4KBTerminology</rdfs:isDefinedBy>
  </owl:Class>








