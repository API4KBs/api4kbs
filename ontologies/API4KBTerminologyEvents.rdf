  <owl:Class
    rdf:about="http://www.omg.org/spec/API4KB/API4KBTerminology/AddingEvent-FocusedOMSItemToImmutableKB">
    <skos:definition rdf:datatype="&xsd;string">operation event adding a Knowledge Resource that is a focused OMS Item to an immutable KB</skos:definition>
    <rdfs:subClassOf
      rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/AddingEvent-KnowledgeResourceToImmutableKB"/>
    <rdfs:subClassOf>
