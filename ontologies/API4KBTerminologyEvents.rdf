        <owl:someValuesFrom
          rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/GeneralizedExpression"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>



  <owl:Class rdf:about="http://www.omg.org/spec/API4KB/API4KBTerminology/OutStreaming">
    <rdfs:label>outstreaming event</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">Action Event of transforming a knowledge encoding
