  <owl:Class rdf:about="http://www.omg.org/spec/API4KB/API4KBTerminology/PublicOperationEvent">
    <rdfs:label>public operation event</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">event of a public operation.</skos:definition>
    <owl:subClassOf>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description
            rdf:about="http://www.omg.org/spec/API4KB/API4KBTerminology/OperationEvent"/>
          <owl:Restriction>
            <owl:onProperty
              rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/actualizes"/>
            <owl:onClass rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/Proficiency"/>
            <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger"
              >1</owl:qualifiedCardinality>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty
              rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/isEventof"/>
            <owl:onClass
              rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/PublicOperation"/>
            <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger"
              >1</owl:qualifiedCardinality>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:subClassOf>
    <skos:example>Example: add Asset to an immutable KB to generate a new immutable KB. </skos:example>
  </owl:Class>

  <!-- script output -->
  

  <owl:Class rdf:about="http://www.omg.org/spec/API4KB/API4KBTerminology/QueryingEvent">
    <rdfs:label>querying event</rdfs:label>
    <skos:definition>action event that applies a query to a focused OMS resource producing a collection of answer
      substitutions.</skos:definition>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/ActionEvent"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/hasFirstInput"/>
        <owl:onClass rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/QueryResource"/>
        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/hasSecondInput"/>
        <owl:onClass rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/FocusedOMSResource"/>
        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/hasOutput"/>
        <owl:onClass rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/AnswerSubstitutionCollection"/>
        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
  <!-- Lowering Events -->

  <owl:Class rdf:about="http://www.omg.org/spec/API4KB/API4KBTerminology/Lowering">
    <rdfs:label>lowering event</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">Action Event whose input is a Knowledge Resource and
      whose output is a Knowledge Resource of the same &quot;type&quot; (i.e. OMS, Query, Result)
      and belongs to the next lower level of abstraction in the Knowledge Resource
      hierarchy.</skos:definition>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/ActionEvent"/>
  </owl:Class>
    
  <owl:Class rdf:about="http://www.omg.org/spec/API4KB/API4KBTerminology/Expressing">
