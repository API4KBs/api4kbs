<rdf:RDF xml:base="http://www.omg.org/spec/API4KB/API4KBTerminologyEvents/"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:dct="http://purl.org/dc/terms/"
