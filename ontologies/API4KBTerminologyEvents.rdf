  <!-- Public Operations -->
  
  <owl:Class
    rdf:about="http://www.omg.org/spec/API4KB/API4KBTerminology/AddingEvent-KnowledgeResourceToImmutableKB">
    <skos:definition rdf:datatype="&xsd;string">abstract class of Operation Events adding a Knowlede
      Resource to an immutable KB</skos:definition>
    <skos:note>Members of this class are events of some operation of type
      &apos;AddKnowledgeResourceToImmutableKB&apos;</skos:note>
    <rdfs:subClassOf
      rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/PublicOperationEvent"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/actualizes"/>
        <owl:onClass
          rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/AddingProcifiency-KnowledgeResourceToImmutableKB"/>
        <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger"
          >1</owl:qualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/isEventof"/>
        <owl:allValuesFrom
          rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/AddingOperation-KnowledgeResourceToImmutableKB"
        />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/hasFirstInput"/>
        <owl:onClass rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/KnowledgeResource"/>
