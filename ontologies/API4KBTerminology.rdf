    <owl:Class rdf:about="&apiforkb-trm;BasicLogicalEnvironment">
        <rdfs:subClassOf rdf:resource="&apiforkb-trm;ImmutableFocusedLogicalEnvironment"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/API4KB/Terminology/Client -->

    <owl:Class rdf:about="&apiforkb-trm;Client">
        <rdfs:label>Client</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
