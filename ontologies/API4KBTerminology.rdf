            <owl:Restriction>
                <owl:onProperty rdf:resource="&apiforkb-trm;serializationOf"/>
                <owl:someValuesFrom rdf:resource="&apiforkb-trm;QueryLanguage"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">serialization of a query language.</skos:definition>
        <rdfs:isDefinedBy>http://www.omg.org/spec/API4KB/Terminology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/API4KB/Terminology/Querying -->

    <owl:Class rdf:about="&apiforkb-trm;Querying">
        <rdfs:subClassOf rdf:resource="&apiforkb-trm;Action"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&apiforkb-trm;hasInput"/>
                <owl:someValuesFrom rdf:resource="&apiforkb-trm;OMSResource"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&apiforkb-trm;hasInput"/>
                <owl:someValuesFrom rdf:resource="&apiforkb-trm;QueryResource"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">action of applying a query to a knowledge base.</skos:definition>
        <rdfs:isDefinedBy>http://www.omg.org/spec/API4KB/Terminology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/API4KB/Terminology/Result -->

    <owl:Class rdf:about="&apiforkb-trm;Result">
        <rdfs:label>Result</rdfs:label>
        <rdfs:subClassOf rdf:resource="&apiforkb-trm;GeneralizedExpression"/>
        <rdfs:subClassOf rdf:resource="&apiforkb-trm;ResultResource"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&apiforkb-trm;usesLanguage"/>
                <owl:someValuesFrom rdf:resource="&apiforkb-trm;ResultLanguage"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&apiforkb-trm;usesLanguage"/>
                <owl:allValuesFrom rdf:resource="&apiforkb-trm;ResultLanguage"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">sequence of bindings as a correspondence between distinguished query variables and derived symbols.</skos:definition>
        <rdfs:isDefinedBy>http://www.omg.org/spec/API4KB/Terminology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/API4KB/Terminology/ResultAsset -->

    <owl:Class rdf:about="&apiforkb-trm;ResultAsset">
        <rdfs:label>Result Asset</rdfs:label>
        <rdfs:subClassOf rdf:resource="&apiforkb-trm;KnowledgeAsset"/>
        <rdfs:subClassOf rdf:resource="&apiforkb-trm;ResultResource"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&apiforkb-trm;expressedAs"/>
                <owl:someValuesFrom rdf:resource="&apiforkb-trm;Result"/>
            </owl:Restriction>
