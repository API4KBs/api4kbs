    <!-- http://www.omg.org/spec/API4KB/Terminology/Action -->

    <owl:Class rdf:about="&apiforkb-trm;Action">
        <rdfs:label>Action</rdfs:label>
        <rdfs:subClassOf rdf:resource="&apiforkb-trm;InternalOperation"/>
        <skos:definition rdf:datatype="&xsd;string">atomic Operations.</skos:definition>
        <rdfs:isDefinedBy>http://www.omg.org/spec/API4KB/Terminology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/API4KB/Terminology/AdditionOfKnowledgeResourceToImmutableKB -->

    <owl:Class rdf:about="&apiforkb-trm;AdditionOfKnowledgeResourceToImmutableKB">
        <rdfs:subClassOf rdf:resource="&apiforkb-trm;Proficiency"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&apiforkb-trm;isActualizedBy"/>
                <owl:allValuesFrom rdf:resource="&apiforkb-trm;addingKnowledgeResourceToImmutableKB"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/API4KB/Terminology/Application -->

    <owl:Class rdf:about="&apiforkb-trm;Application">
        <skos:definition rdf:datatype="&xsd;string">TBD</skos:definition>
        <rdfs:isDefinedBy>http://www.omg.org/spec/API4KB/Terminology</rdfs:isDefinedBy>
        <dct:abstract></dct:abstract>
        <owl:disjointUnionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="&apiforkb-trm;Client"/>
            <rdf:Description rdf:about="&apiforkb-trm;KnowledgePlatformManager"/>
            <rdf:Description rdf:about="&apiforkb-trm;Proxy"/>
        </owl:disjointUnionOf>
    </owl:Class>
    


