  <!-- Script Input -->
  
  <!-- Operations -->
  
  <owl:Class rdf:about="&apiforkb-trm;Operation">
    <rdfs:label>operation</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">function (possibly with side-effects) having inputs and outputs that are API4KB or OntoIOp concepts.</skos:definition>
    <skos:note rdf:datatype="&xsd;string">Option 1. the definition as &quot;function&quot; means that its a black-box standard. We may define the post-conditions by a reference decomposition into actions, but there is no normative requirement for applications to execute according to that decomposition.
      
      Option 2. at the other extreme, the decomposition could also be consider normative.
      
      Option 3. something in between.</skos:note>
    <skos:scopeNote rdf:datatype="&xsd;string">Each API4KB Interface is a subclass of Operation.</skos:scopeNote>
    <skos:scopeNote rdf:datatype="&xsd;string">This class contains only API4KB Operations.</skos:scopeNote>
    <skos:editorialNote rdf:datatype="&xsd;string">is there a super-class for Operation in some OMG standard?</skos:editorialNote>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:onProperty rdf:resource="&apiforkb-trm;hasEvent"/>
            <owl:someValuesFrom rdf:resource="&apiforkb-trm;OperationEvent"/>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty rdf:resource="&apiforkb-trm;hasEvent"/>
            <owl:allValuesFrom rdf:resource="&apiforkb-trm;OperationEvent"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <owl:disjointUnionOf rdf:parseType="Collection">
      <rdf:Description rdf:about="&apiforkb-trm;SideEffectFreeOperation"/>
      <rdf:Description rdf:about="&apiforkb-trm;SideEffectfulOperation"/>
    </owl:disjointUnionOf>
    <owl:disjointUnionOf rdf:parseType="Collection">
      <rdf:Description rdf:about="&apiforkb-trm;InternalOperation"/>
      <rdf:Description rdf:about="&apiforkb-trm;PublicOperation"/>
    </owl:disjointUnionOf>
  </owl:Class>
  
  <!-- Internal Operations -->
  
  <owl:Class rdf:about="&apiforkb-trm;InternalOperation">
    <rdfs:label>internal operation</rdfs:label>
    <skos:definition>operation offered by the knowledge platform to the proxy.</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;Operation"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;hasEvent"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-trm;InternalOperationEvent"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith
      rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/PublicOperation"/>
  </owl:Class>
  
  <!-- Actions -->

  <owl:Class rdf:about="&apiforkb-trm;Action">
    <rdfs:label>action</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">an atomic operation</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;InternalOperation"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;hasEvent"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-trm;ActionEvent"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
  <owl:Class rdf:about="http://www.omg.org/spec/API4KB/API4KBTerminology/QueryingAction">
    <rdfs:label>querying action</rdfs:label>
    <skos:definition>action that maps queries and focused OMS resource to collections of answer
      substitutions.</skos:definition>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/Action"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/mapsFromOne"/>
        <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/QueryResource"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/mapsFromTwo"/>
        <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/FocusedOMSResource"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/mapsTo"/>
        <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/AnswerSubstitutionCollection"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
  <owl:Class rdf:about="&apiforkb-trm;LowerAction">
    <rdfs:label>lower action</rdfs:label>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;Action"/>
  </owl:Class>
  
  <owl:Class rdf:about="&apiforkb-trm;Manifest">
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;LowerAction"/>
  </owl:Class>
  
  <owl:Class rdf:about="&apiforkb-trm;Marshall">
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;LowerAction"/>
  </owl:Class>
  
  <owl:Class rdf:about="&apiforkb-trm;OutStream">
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;LowerAction"/>
  </owl:Class>
  
  <owl:Class rdf:about="&apiforkb-trm;LiftingAction">
    <rdfs:label>lift action</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">Action whose input is a Knowledge Resource and whose output is a Knowledge Resource of the same &quot;type&quot; (i.e. OMS, Query, Result) and belongs to the next higher level of abstraction in the Knowledge Resource hierarchy.</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;Action"/>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;UnaryFunction"/>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;SideEffectFreeOperation"/>
    <owl:disjointUnionOf rdf:parseType="Collection">
      <rdf:Description rdf:about="&apiforkb-trm;Conceptualize"/>
      <rdf:Description rdf:about="&apiforkb-trm;InStream"/>
      <rdf:Description rdf:about="&apiforkb-trm;Parse"/>
      <rdf:Description rdf:about="&apiforkb-trm;Unmarshall"/>
    </owl:disjointUnionOf>
  </owl:Class>
    
  <owl:Class rdf:about="&apiforkb-trm;Parse">
    <rdfs:label>parse</rdfs:label>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;LiftingAction"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;hasCoDomainMember"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-trm;GeneralizedExpression"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;hasDomainMember"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-trm;KnowledgeManifestation"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
  <owl:Class rdf:about="http://www.omg.org/spec/API4KB/API4KBTerminology/UnmarshallingAction">
    <rdfs:label>unmarshalling action</rdfs:label>
    <skos:definition>lifting action that maps knowledge encodings into knowledge manifestations.</skos:definition>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/LiftingAction"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/mapsFrom"/>
        <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/KnowledgeEncoding"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/mapsTo"/>
        <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/KnowledgeManifestation"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  

  <!-- Public Operations -->
  
  <owl:Class rdf:about="&apiforkb-trm;AddingOperation-KnowledgeResourceToImmutableKB">
    <rdfs:label>operation adding knowledge resources to immutable KBs</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">abstract class of operations adding knowlede
      resources to immutable KBs</skos:definition>
    <dct:abstract/>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;PublicOperation"/>
  </owl:Class>
  
  <owl:Class rdf:about="&apiforkb-trm;AddingOperation-KnowledgeResourceToMutableKB">
    <rdfs:label>operation adding knowledge resources to mutable KBs</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">abstract class of operations adding knowlede
      resources to mutable KBs</skos:definition>
    <dct:abstract/>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;PublicOperation"/>
  </owl:Class>
  
  
  <owl:Class rdf:about="&apiforkb-trm;AddingOperation-QueryItemToImmutableKB">
    <rdfs:label>operation adding knowledge resources that are query items to immutable KBs</rdfs:label>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;AddingOperation-KnowledgeResourceToImmutableKB"/>
  </owl:Class>
  
  
  <owl:Class rdf:about="&apiforkb-trm;AddingOperation-FocusedOMSItemToImmutableKB">
    <skos:definition rdf:datatype="&xsd;string">operation adding knowledge resources that are FocusedOMSs Item to immutable KBs</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;AddingOperation-KnowledgeResourceToImmutableKB"/>
  </owl:Class>
  
</rdf:RDF>