            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">knowledge asset for query results.</skos:definition>
    </owl:Class>
    
    <owl:Class rdf:about="&apiforkb-trm;ResultEncoding">
        <rdfs:label>Result Encoding</rdfs:label>
