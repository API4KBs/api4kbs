    </owl:Class>
    


    <!-- http://www.omg.org/spec/API4KB/Terminology/OMSEncoding -->

    <owl:Class rdf:about="&apiforkb-trm;OMSEncoding">
        <rdfs:label>OMS Encoding</rdfs:label>
        <rdfs:subClassOf rdf:resource="&apiforkb-trm;KnowledgeEncoding"/>
        <rdfs:subClassOf rdf:resource="&apiforkb-trm;OMSResource"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&apiforkb-trm;encodingOf"/>
                <owl:someValuesFrom rdf:resource="&apiforkb-trm;OMSManifestation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">byte (or bit) sequence that is the encoding of a OMS Manifestation in some character encoding system(s).</skos:definition>
        <rdfs:isDefinedBy>http://www.omg.org/spec/API4KB/Terminology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/API4KB/Terminology/OMSItem -->

    <owl:Class rdf:about="&apiforkb-trm;OMSItem">
        <rdfs:label>OMS Item</rdfs:label>
        <rdfs:subClassOf rdf:resource="&apiforkb-trm;KnowledgeItem"/>
        <rdfs:subClassOf rdf:resource="&apiforkb-trm;OMSResource"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&apiforkb-trm;localCopyOf"/>
                <owl:someValuesFrom rdf:resource="&apiforkb-trm;OMSEncoding"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">physical instance of an OMS Encoding, e.g. a file on a hard drive, a Java String in memory.</skos:definition>
        <rdfs:isDefinedBy>http://www.omg.org/spec/API4KB/Terminology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/API4KB/Terminology/OMSManifestation -->

    <owl:Class rdf:about="&apiforkb-trm;OMSManifestation">
        <rdfs:label>OMS Manifestation</rdfs:label>
        <rdfs:subClassOf rdf:resource="&apiforkb-trm;KnowledgeManifestation"/>
        <rdfs:subClassOf rdf:resource="&apiforkb-trm;OMSResource"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&apiforkb-trm;manifestationOf"/>
                <owl:someValuesFrom rdf:resource="&ontoiop-trm;OMS"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
