    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
   -->
  
  <owl:ObjectProperty rdf:about="&apiforkb-ev;hasInput">
    <rdfs:label>has input</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">input of an event</skos:definition>
    <rdfs:domain rdf:resource="&apiforkb-ev;Event"/>
    <owl:inverseOf rdf:resource="&apiforkb-ev;isInputOf"/>
    <owl:propertyChainAxiom rdf:parseType="Collection">
      <rdf:Description rdf:about="&apiforkb-try;isEventOf"/>
      <rdf:Description rdf:about="&apiforkb-try;hasInput"/>
    </owl:propertyChainAxiom>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:about="&apiforkb-ev;isInputOf">
    <rdfs:label>is input of</rdfs:label>
  </owl:ObjectProperty>  

  <owl:ObjectProperty rdf:about="&apiforkb-ev;hasUnaryInput">
    <rdfs:label>has unary input</rdfs:label>
    <rdf:type rdf:resource="&owl;FunctionalProperty"/>
    <rdfs:subPropertyOf rdf:resource="&apiforkb-ev;hasInput"/>
    <owl:inverseOf rdf:resource="&apiforkb-ev;isUnaryInputOf"/>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:about="&apiforkb-ev;hasOutput">
    <rdf:type rdf:resource="&owl;FunctionalProperty"/>
    <rdfs:label>has output</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">output of an event</skos:definition>
    <rdfs:domain rdf:resource="&apiforkb-ev;Event"/>
    <owl:inverseOf rdf:resource="&apiforkb-ev;isOutputOf"/>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:about="&apiforkb-ev;isOutputOf">
    <rdfs:label>is output of</rdfs:label>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:about="&apiforkb-ev;hasKPOutput">
    <rdfs:label>has knowledge platform output</rdfs:label>
    <rdfs:range rdf:resource="&apiforkb-trm;KnowledgePlatform"/>
    <rdfs:subPropertyOf rdf:resource="&apiforkb-ev;hasOutput"/>
    <owl:inverseOf rdf:resource="&apiforkb-ev;isKPOutputOf"/>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:about="&apiforkb-ev;isKPOutputOf">
    <rdfs:label>is knowledge platform that is output of</rdfs:label>
  </owl:ObjectProperty>
  
  
  
  
  <owl:ObjectProperty rdf:about="&apiforkb-ev;appliesQuery">
    <rdfs:label>applies query resource</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">TBD</skos:definition>
    <rdfs:domain rdf:resource="&apiforkb-ev;QueryingEvent"/>
    <rdfs:range rdf:resource="&apiforkb-trm;QuerySource"/>    
    <rdfs:subPropertyOf rdf:resource="&apiforkb-ev;hasInput"/>
    <skos:note>need inverse</skos:note>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="&apiforkb-ev;appliesQueryTo">
    <rdfs:label>applies query to resource</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">TBD</skos:definition>
    <rdf:type rdf:resource="&owl;FunctionalProperty"/>
    <rdfs:domain rdf:resource="&apiforkb-ev;QueryingEvent"/>
    <rdfs:range rdf:resource="&apiforkb-trm;KnowledgeSource"/>    
    <rdfs:subPropertyOf rdf:resource="&apiforkb-ev;hasInput"/>
    <skos:note>need inverse</skos:note>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:about="&apiforkb-ev;adds">
    <rdfs:label>adds resource</rdfs:label>
    <rdfs:domain rdf:resource="&apiforkb-ev;InternalAddingEvent"/>
    <rdfs:range rdf:resource="&apiforkb-trm;KnowledgeSource"/>
    <rdfs:subPropertyOf rdf:resource="&apiforkb-ev;hasInput"/>
    <skos:note>need inverse</skos:note>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="&apiforkb-ev;addsTo">
    <rdfs:label>adds resource</rdfs:label>
    <rdf:type rdf:resource="&owl;FunctionalProperty"/>
    <rdfs:domain rdf:resource="&apiforkb-ev;InternalAddingEvent"/>
    <rdfs:range rdf:resource="&apiforkb-trm;KnowledgeSource"/>
    <rdfs:subPropertyOf rdf:resource="&apiforkb-ev;hasInput"/>
    <skos:note>need inverse</skos:note>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="&apiforkb-ev;hasOutputFromInStreaming">
    <rdfs:label>has output from instreaming</rdfs:label>
    <rdfs:domain rdf:resource="&apiforkb-ev;InStreamEvent"/>
    <rdfs:range rdf:resource="&apiforkb-trm;KnowledgeEncoding"/>
    <rdfs:subPropertyOf rdf:resource="&apiforkb-ev;hasOutput"/>
    <skos:note>need inverse</skos:note>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:about="&apiforkb-ev;hasOutputFromUnmarshalling">
    <rdfs:label>has output from unmarshalling</rdfs:label>
    <rdfs:domain rdf:resource="&apiforkb-ev;UnmarshallingEvent"/>
    <rdfs:range rdf:resource="&apiforkb-trm;KnowledgeManifestation"/>
    <rdfs:subPropertyOf rdf:resource="&apiforkb-ev;hasOutput"/>
    <skos:note>need inverse</skos:note>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="&apiforkb-ev;hasOutputFromParsing">
    <rdfs:label>has output from parsing</rdfs:label>
    <rdfs:domain rdf:resource="&apiforkb-ev;ParsingEvent"/>
    <rdfs:range rdf:resource="&apiforkb-trm;GeneralizedExpression"/>
    <rdfs:subPropertyOf rdf:resource="&apiforkb-ev;hasOutput"/>
    <skos:note>need inverse</skos:note>
  </owl:ObjectProperty>
    
  <owl:ObjectProperty rdf:about="&apiforkb-ev;hasOutputFromConceptualizing">
    <rdfs:label>has output from conceptualizing</rdfs:label>
    <rdfs:domain rdf:resource="&apiforkb-ev;ConceptualizingEvent"/>
    <rdfs:range rdf:resource="&apiforkb-trm;KnowledgeAsset"/>
    <rdfs:subPropertyOf rdf:resource="&apiforkb-ev;hasOutput"/>
    <skos:note>need inverse</skos:note>
  </owl:ObjectProperty>
  
  
  
  <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

  <owl:Class rdf:about="&apiforkb-ev;Event">
    <rdfs:label>event</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">event of an API4KB Operation.</skos:definition>
    <skos:example rdf:datatype="&xsd;string">Example: the adding of an Asset to an immutable KB to generate a new immutable KB,
      executed by some application. </skos:example>
    <owl:equivalentClass> 
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description
            rdf:about="&owl;Thing"/>
          <owl:Restriction>
            <owl:onProperty
              rdf:resource="&apiforkb-trm;isEventOf"/>
            <owl:someValuesFrom
              rdf:resource="&apiforkb-op;Operation"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <owl:disjointUnionOf rdf:parseType="Collection">
      <rdf:Description
        rdf:about="&apiforkb-ev;InternalEvent"/>
      <rdf:Description
        rdf:about="&apiforkb-ev;PublicEvent"/>
    </owl:disjointUnionOf>
  </owl:Class>
  
  <owl:Class rdf:about="&apiforkb-ev;SideEffectFreeEvent">
    <rdfs:label>side-effect-free event</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">event of a side-effect-free operation</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-ev;Event"/>
    <owl:equivalentClass> 
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description
            rdf:about="&owl;Thing"/>
          <owl:Restriction>
            <owl:onProperty
              rdf:resource="&apiforkb-trm;isEventOf"/>
            <owl:someValuesFrom
              rdf:resource="&apiforkb-op;SideEffectFreeOperation"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  
  <owl:Class rdf:about="&apiforkb-ev;SideEffectfulEvent">
    <rdfs:label>side-effectful event</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">event of a side-effectful operation</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-ev;Event"/>
    <owl:equivalentClass> 
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description
            rdf:about="&owl;Thing"/>
          <owl:Restriction>
            <owl:onProperty
              rdf:resource="&apiforkb-trm;isEventOf"/>
            <owl:someValuesFrom
              rdf:resource="&apiforkb-op;SideEffectfulOperation"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>


  <owl:Class rdf:about="&apiforkb-ev;UnaryEvent">
    <rdfs:label>unary event</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">unary operation</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-ev;Event"/>
    <owl:equivalentClass> 
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description
            rdf:about="&owl;Thing"/>
          <owl:Restriction>
            <owl:onProperty
              rdf:resource="&apiforkb-trm;isEventOf"/>
            <owl:someValuesFrom
              rdf:resource="&apiforkb-op;UnaryOperation"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-ev;hasUnaryInput"/>
        <owl:someValuesFrom rdf:resource="&owl;Thing"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class> 
  

  <owl:Class rdf:about="&apiforkb-ev;InternalEvent">
    <rdfs:label>internal event</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">event of an Internal Operation.</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-ev;Event"/>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description
            rdf:about="&apiforkb-ev;Event"/>
          <owl:Restriction>
            <owl:onProperty
              rdf:resource="&apiforkb-trm;isEventOf"/>
            <owl:someValuesFrom
              rdf:resource="&apiforkb-op;InternalOperation"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <owl:disjointWith
      rdf:resource="&apiforkb-ev;PublicEvent"/>
  </owl:Class>
  
  <owl:Class rdf:about="&apiforkb-ev;ActionEvent">
    <rdfs:label>action event</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">event of an Action.</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-ev;InternalEvent"/>
    <rdfs:subClassOf rdf:resource="&apiforkb-ev;UnaryEvent"/>
    <owl:equivalentClass> 
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description
            rdf:about="&apiforkb-ev;InternalEvent"/>
          <owl:Restriction>
            <owl:onProperty
              rdf:resource="&apiforkb-trm;isEventOf"/>
            <owl:someValuesFrom
              rdf:resource="&apiforkb-op;Action"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>


  <owl:Class rdf:about="&apiforkb-ev;PublicEvent">
    <rdfs:label>public event</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">event of a public operation.</skos:definition>
    <skos:example rdf:datatype="&xsd;string">Example: add Asset to an immutable KB to generate a new immutable KB. </skos:example>
    <rdfs:subClassOf rdf:resource="&apiforkb-ev;Event"/>
    <owl:subClassOf>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description
            rdf:about="&apiforkb-ev;Event"/>
          <owl:Restriction>
            <owl:onProperty
              rdf:resource="&apiforkb-trm;actualizes"/>
            <owl:someValuesFrom rdf:resource="&apiforkb-pro;Proficiency"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:subClassOf>
    <owl:equivalentClass> 
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description
            rdf:about="&apiforkb-ev;Event"/>
          <owl:Restriction>
            <owl:onProperty
              rdf:resource="&apiforkb-trm;isEventOf"/>
            <owl:someValuesFrom
              rdf:resource="&apiforkb-op;PublicOperation"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>

  <owl:Class rdf:about="&apiforkb-ev;LoweringEvent">
    <rdfs:label>lowering event</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">event of a lowering action.</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-ev;ActionEvent"/>
    <rdfs:subClassOf rdf:resource="&apiforkb-ev;SideEffectFreeEvent"/>
    <owl:equivalentClass> 
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description
            rdf:about="&apiforkb-ev;ActionEvent"/>
          <owl:Restriction>
            <owl:onProperty
              rdf:resource="&apiforkb-trm;isEventOf"/>
            <owl:someValuesFrom
              rdf:resource="&apiforkb-op;LoweringAction"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-ev;hasInput"/>
        <owl:someValuesFrom
          rdf:resource="&apiforkb-trm;KnowledgeResource"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-ev;hasOutput"/>
        <owl:someValuesFrom
          rdf:resource="&apiforkb-trm;KnowledgeResource"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointUnionOf rdf:parseType="Collection">
      <rdf:Description rdf:about="&apiforkb-ev;OutStreamingEvent"/>
      <rdf:Description rdf:about="&apiforkb-ev;MarshallingEvent"/>
      <rdf:Description rdf:about="&apiforkb-ev;ManifestingEvent"/>
      <rdf:Description rdf:about="&apiforkb-ev;ExpressingEvent"/>
    </owl:disjointUnionOf>
  </owl:Class>
    
  <owl:Class rdf:about="&apiforkb-ev;ExpressingEvent">
    <rdfs:label>expressing event</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">lowering event transforming a knowledge asset into
      a generalized expression.</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-ev;LoweringEvent"/>
    <owl:equivalentClass> 
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description
            rdf:about="&apiforkb-ev;LoweringEvent"/>
          <owl:Restriction>
            <owl:onProperty
              rdf:resource="&apiforkb-trm;isEventOf"/>
            <owl:someValuesFrom
              rdf:resource="&apiforkb-op;ExpressingAction"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-ev;hasInput"/>
        <owl:someValuesFrom
          rdf:resource="&apiforkb-trm;KnowledgeAsset"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-ev;hasOutput"/>
        <owl:someValuesFrom
          rdf:resource="&apiforkb-trm;GeneralizedExpression"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
  <owl:Class rdf:about="&apiforkb-ev;ManifestingEvent">
    <rdfs:label>manifesting event</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">lowering event transforming a generalized
      expression into a knowledge manifestation.</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-ev;LoweringEvent"/>
    <owl:equivalentClass> 
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description
            rdf:about="&apiforkb-ev;LoweringEvent"/>
          <owl:Restriction>
            <owl:onProperty
              rdf:resource="&apiforkb-trm;isEventOf"/>
            <owl:someValuesFrom
              rdf:resource="&apiforkb-op;ManifestingAction"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-ev;hasInput"/>
        <owl:someValuesFrom
          rdf:resource="&apiforkb-trm;GeneralizedExpression"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-ev;hasOutput"/>
        <owl:someValuesFrom
          rdf:resource="&apiforkb-trm;KnowledgeManifestation"/>
      </owl:Restriction>
    </rdfs:subClassOf>    
  </owl:Class>
  
  <owl:Class rdf:about="&apiforkb-ev;MarshallingEvent">
    <rdfs:label>marshalling event</rdfs:label>
    <rdfs:subClassOf rdf:resource="&apiforkb-ev;LoweringEvent"/>
    <skos:definition rdf:datatype="&xsd;string">lowering event transforming a knowledge
      manifestation into a knowledge encoding.</skos:definition>
    <owl:equivalentClass> 
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description
            rdf:about="&apiforkb-ev;LoweringEvent"/>
          <owl:Restriction>
            <owl:onProperty
              rdf:resource="&apiforkb-trm;isEventOf"/>
            <owl:someValuesFrom
              rdf:resource="&apiforkb-op;MarshallingAction"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-ev;hasInput"/>
        <owl:someValuesFrom
          rdf:resource="&apiforkb-trm;KnowledgeManifestation"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-ev;hasOutput"/>
        <owl:someValuesFrom
          rdf:resource="&apiforkb-trm;KnowledgeEncoding"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="&apiforkb-ev;OutStreamingEvent">
    <rdfs:label>outstreaming event</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">lowering event transforming a knowledge encoding into a
