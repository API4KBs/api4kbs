  <owl:Ontology rdf:about="&apiforkb-ev;/">
    <rdfs:label>API4KB Event Terminology Ontology</rdfs:label>
    
    
    <!-- Curation and Rights Metadata for the API4KB Event Terminology Ontology -->
    
    <sm:copyright rdf:datatype="&xsd;string">Copyright (c) 2014 Athan Services
      Copyright (c) 2014 Thematix Partners LLC
      Copyright (c) 2014 Object Management Group, Inc.</sm:copyright>
    <dct:license rdf:datatype="&xsd;anyURI">http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense</dct:license>
    
    
    <!-- Ontology/File-Level Metadata for the API4KB Event Terminology Ontology -->
    
    <sm:filename rdf:datatype="&xsd;string">API4KBTerminologyEvent.rdf</sm:filename>
    <sm:fileAbbreviation rdf:datatype="&xsd;string">api4kb-ev</sm:fileAbbreviation>
    <owl:versionIRI rdf:resource="http://www.omg.org/spec/API4KB/20150201/API4KBTerminology/API4KBEvent/"/>
    <sm:fileAbstract rdf:datatype="&xsd;string">This ontology covers terminology used for describing events defined by the API4KB specification.</sm:fileAbstract>
    
