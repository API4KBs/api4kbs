        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-ev;hasInput"/>
        <owl:someValuesFrom
          rdf:resource="&apiforkb-trm;KnowledgeEncoding"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
