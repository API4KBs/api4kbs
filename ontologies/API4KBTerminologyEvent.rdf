            rdf:about="&apiforkb-ev;OperationEvent"/>
          <owl:Restriction>
            <owl:onProperty
              rdf:resource="&apiforkb-trm;isEventof"/>
            <owl:someValuesFrom
              rdf:resource="&apiforkb-op;InternalOperation"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <owl:disjointWith
      rdf:resource="&apiforkb-ev;PublicOperationEvent"/>
  </owl:Class>
  
  <owl:Class rdf:about="&apiforkb-ev;ActionEvent">
