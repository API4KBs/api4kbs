            <owl:someValuesFrom
              rdf:resource="&apiforkb-op;PublicOperation"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>

  <!-- Script Output -->
  

  <owl:Class rdf:about="&apiforkb-ev;QueryingEvent">
    <rdfs:label>querying event</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">action event that applies a query to a focused resource producing a collection of answer
      substitutions.</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-ev;ActionEvent"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;hasFirstInput"/>
        <owl:someValuesFrom rdf:resource="&apiforkb-trm;QueryResource"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;hasSecondInput"/>
        <owl:someValuesFrom rdf:resource="&apiforkb-trm;FocusedResource"/>
