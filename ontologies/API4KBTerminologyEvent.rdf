          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  
  <!-- Public Operation Event -->  

  <owl:Class rdf:about="&apiforkb-ev;PublicOperationEvent">
    <rdfs:label>public operation event</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">event of a public operation.</skos:definition>
    <skos:example rdf:datatype="&xsd;string">Example: add Asset to an immutable KB to generate a new immutable KB. </skos:example>
    <rdfs:subClassOf rdf:resource="&apiforkb-ev;OperationEvent"/>
    <owl:subClassOf>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description
            rdf:about="&apiforkb-ev;OperationEvent"/>
          <owl:Restriction>
            <owl:onProperty
              rdf:resource="&apiforkb-trm;actualizes"/>
            <owl:someValuesFrom rdf:resource="&apiforkb-pro;Proficiency"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:subClassOf>
    <owl:equivalentClass> 
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
