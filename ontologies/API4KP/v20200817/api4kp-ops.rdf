	<owl:Class rdf:about="&api4kp-ops;AbridgmentTask">
		<rdfs:subClassOf rdf:resource="&api4kp-ops;ParaphraseTask"/>
		<rdfs:label>abdrigment task</rdfs:label>
		<skos:definition>paraphrase that eliminates element of an expression that are not essential to reconstruct its meaning</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&api4kp-ops;CarvingTask">
		<rdfs:subClassOf rdf:resource="&api4kp-ops;SelectionTask"/>
		<rdfs:label>carving task</rdfs:label>
		<skos:definition>selection based on the inclusion of fragments that evoke certain concepts</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&api4kp-ops;CherryPickingTask">
		<rdfs:subClassOf rdf:resource="&api4kp-ops;SelectionTask"/>
		<rdfs:label>cherry-picking task</rdfs:label>
		<skos:definition>selection task where the concepts to be included are enumerated explicitly</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&api4kp-ops;ClassificationTask">
		<rdfs:subClassOf rdf:resource="&api4kp-ops;LogicBasedReasoningTask"/>
		<rdfs:label>classification task</rdfs:label>
		<skos:note>reasoning Task such that, given an individual and a set of concepts (classes), determines which concepts (classes) that individual is an instance (member) of</skos:note>
	</owl:Class>
	
	<owl:Class rdf:about="&api4kp-ops;ClearRepositoryLocationTask">
		<rdfs:subClassOf rdf:resource="&api4kp-ops;KnowledgeArtifactRepositoryTask"/>
		<rdfs:label>clear repository location task</rdfs:label>
		<skos:definition>ensures that no artifact is stored at a given location</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&api4kp-ops;ComplexityDetectionTask">
		<rdfs:subClassOf rdf:resource="&api4kp-ops;KnowledgeAnalyticTask"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&api4kp-ops;hasSubtask"/>
				<owl:someValuesFrom rdf:resource="&api4kp-ops;ParsingTask"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&api4kp-ops;involvesRole"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:intersectionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&api4kp-ops;Product">
							</rdf:Description>
							<owl:Restriction>
								<owl:onProperty rdf:resource="&api4kp-ops;isFilledBy"/>
								<owl:someValuesFrom rdf:resource="&api4kp-krr;ComputationalComplexityClass"/>
							</owl:Restriction>
						</owl:intersectionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>complexity detection task</rdfs:label>
		<skos:definition>analytic task that detects the computational complexity (space and/or time) of a given knowledge expression</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&api4kp-ops;ConfigurationParameter">
		<rdfs:subClassOf rdf:resource="&api4kp-ops;KnowledgeProcessingTaskRole"/>
		<rdfs:label>configuration parameter</rdfs:label>
		<skos:definition>role played by a value, selected from a &apos;region&apos; of admissible values, that can remove a degree of freedom from the execution of a (knowledge processing) task, without changing the nature of the task</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&api4kp-ops;ConsistencyCheckTask">
		<rdfs:subClassOf rdf:resource="&api4kp-ops;LogicBasedReasoningTask"/>
		<rdfs:label>consistency checking task</rdfs:label>
		<rdfs:seeAlso>https://en.wikipedia.org/wiki/Consistency</rdfs:seeAlso>
		<skos:definition>reasoning task that determines whether a knowledge base is inconsistent, i.e. it allows derivation of a logical contradication</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&api4kp-ops;DecodingTask">
		<rdfs:subClassOf rdf:resource="&api4kp-ops;LiftingTask"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&api4kp-ops;involvesRole"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:intersectionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&api4kp-ops;Input">
							</rdf:Description>
							<owl:Restriction>
								<owl:onProperty rdf:resource="&api4kp-ops;isFilledBy"/>
								<owl:someValuesFrom rdf:resource="&api4kp;EncodedKnowledgeExpression"/>
							</owl:Restriction>
						</owl:intersectionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&api4kp-ops;involvesRole"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:intersectionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&api4kp-ops;Product">
							</rdf:Description>
							<owl:Restriction>
								<owl:onProperty rdf:resource="&api4kp-ops;isFilledBy"/>
								<owl:someValuesFrom rdf:resource="&api4kp;ConcreteKnowledgeExpression"/>
							</owl:Restriction>
						</owl:intersectionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>decoding task</rdfs:label>
		<skos:definition>the act of recreating a concrete expression from its encoded version</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&api4kp-ops;DependencyResolutionTask">
		<rdfs:subClassOf rdf:resource="&api4kp-ops;KnowledgeResourceDiscoveryTask"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&api4kp-ops;involvesRole"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:intersectionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&api4kp-ops;Input">
							</rdf:Description>
							<owl:Restriction>
								<owl:onProperty rdf:resource="&api4kp-ops;isFilledBy"/>
								<owl:someValuesFrom rdf:resource="&api4kp;KnowledgeResource"/>
							</owl:Restriction>
						</owl:intersectionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&api4kp-ops;involvesRole"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:intersectionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&api4kp-ops;Product">
							</rdf:Description>
							<owl:Restriction>
								<owl:onProperty rdf:resource="&api4kp-ops;isFilledBy"/>
								<owl:someValuesFrom rdf:resource="&api4kp;KnowledgeResource"/>
							</owl:Restriction>
						</owl:intersectionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>dependency resolution task</rdfs:label>
		<skos:definition>knowledge resource discovery task that resolves dependency relationships between knowledge resources</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&api4kp-ops;DiffingTask">
		<rdfs:subClassOf rdf:resource="&api4kp-ops;KnowledgeAnalyticTask"/>
		<rdfs:label>diffing task</rdfs:label>
		<skos:definition>analytic task that detects the differences between two knowledge expressions</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&api4kp-ops;EncodingTask">
		<rdfs:subClassOf rdf:resource="&api4kp-ops;LoweringTask"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&api4kp-ops;involvesRole"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:intersectionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&api4kp-ops;Input">
							</rdf:Description>
							<owl:Restriction>
								<owl:onProperty rdf:resource="&api4kp-ops;isFilledBy"/>
								<owl:someValuesFrom rdf:resource="&api4kp-lang;Encoding"/>
							</owl:Restriction>
						</owl:intersectionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&api4kp-ops;involvesRole"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:intersectionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&api4kp-ops;Input">
							</rdf:Description>
							<owl:Restriction>
								<owl:onProperty rdf:resource="&api4kp-ops;isFilledBy"/>
								<owl:someValuesFrom rdf:resource="&api4kp;ConcreteKnowledgeExpression"/>
							</owl:Restriction>
						</owl:intersectionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&api4kp-ops;involvesRole"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:intersectionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&api4kp-ops;Product">
							</rdf:Description>
							<owl:Restriction>
								<owl:onProperty rdf:resource="&api4kp-ops;isFilledBy"/>
								<owl:someValuesFrom rdf:resource="&api4kp;EncodedKnowledgeExpression"/>
							</owl:Restriction>
						</owl:intersectionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>encoding task</rdfs:label>
		<skos:definition></skos:definition>
		<skos:definition>lowering task that applies an encoding to a concrete expression</skos:definition>
		<skos:example>converting characters into sequences of bits, according to an encoding method, so that they can be written to a RAM, ROM or magnetic disk</skos:example>
	</owl:Class>
	
	<owl:Class rdf:about="&api4kp-ops;EntailmentCheckTask">
		<rdfs:subClassOf rdf:resource="&api4kp-ops;LogicBasedReasoningTask"/>
		<rdfs:label>entailment check test</rdfs:label>
		<skos:definition>reasoning task that determines whether a given working knowledge base is entailed by a reference working knowledge base through logical consequence</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&api4kp-ops;Explanation">
		<rdfs:subClassOf rdf:resource="&api4kp-ops;KnowledgeProcessingTaskRole"/>
		<rdfs:label>explanation</rdfs:label>
		<skos:definition>role played by an expression, generated as part of the execution of a (knowledge processing) activity, that captures knowledge about the course of that activity and/or its inputs, outputs and substrate</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&api4kp-ops;ExpressionTask">
		<rdfs:subClassOf rdf:resource="&api4kp-ops;LoweringTask"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&api4kp-ops;involvesRole"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:intersectionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&api4kp-ops;Input">
							</rdf:Description>
							<owl:Restriction>
								<owl:onProperty rdf:resource="&api4kp-ops;isFilledBy"/>
								<owl:someValuesFrom rdf:resource="&api4kp-lang;AbstractSyntax"/>
							</owl:Restriction>
						</owl:intersectionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&api4kp-ops;involvesRole"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:intersectionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&api4kp-ops;Input">
							</rdf:Description>
							<owl:Restriction>
								<owl:onProperty rdf:resource="&api4kp-ops;isFilledBy"/>
								<owl:someValuesFrom rdf:resource="&api4kp-lang;Lexicon"/>
							</owl:Restriction>
						</owl:intersectionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&api4kp-ops;involvesRole"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:intersectionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&api4kp-ops;Input">
							</rdf:Description>
							<owl:Restriction>
								<owl:onProperty rdf:resource="&api4kp-ops;isFilledBy"/>
								<owl:someValuesFrom rdf:resource="&api4kp;WorkOfKnowledge"/>
							</owl:Restriction>
						</owl:intersectionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&api4kp-ops;involvesRole"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:intersectionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&api4kp-ops;Product">
							</rdf:Description>
							<owl:Restriction>
								<owl:onProperty rdf:resource="&api4kp-ops;isFilledBy"/>
								<owl:someValuesFrom rdf:resource="&api4kp;AbstractKnowledgeExpression"/>
							</owl:Restriction>
						</owl:intersectionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>expression task</rdfs:label>
		<skos:definition>lowering task that creates an expression in a given language (sytnax and lexicon), from the information content of a work of knowledge</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&api4kp-ops;ExtensionTask">
		<rdfs:subClassOf rdf:resource="&api4kp-ops;InjectionTask"/>
		<rdfs:label>extension task</rdfs:label>
		<skos:definition>injection of a resource into a second resource, which plays the role of &apos;extension&apos;, providing a wrapping context</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&api4kp-ops;ExtractionTask">
		<rdfs:subClassOf rdf:resource="&api4kp-ops;SeparationTask"/>
		<rdfs:label>extraction task</rdfs:label>
		<skos:definition>separation based on a tree-oriented deconstruction, where the resources that are part of a subtree of the underlying structure are returned</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&api4kp-ops;FilteringTask">
		<rdfs:subClassOf rdf:resource="&api4kp-ops;SimplificationTask"/>
		<rdfs:label>censoring task</rdfs:label>
		<skos:definition>simplification aimed at removing one or more concepts, and their transitive associations, from the input work of knowledge</skos:definition>
		<skos:note>The expression of the resulting WoK will not contain any linguistic element that evokes the redacted concepts.</skos:note>
	</owl:Class>
	
	<owl:Class rdf:about="&api4kp-ops;FusionTask">
		<rdfs:subClassOf rdf:resource="&api4kp-ops;KnowledgeReshapingTask"/>
		<rdfs:label>fusion task</rdfs:label>
		<skos:definition>reshaping task that consists in combining two resources into a new, distinct resource</skos:definition>
		<skos:note>conceptually equivalent to structuring two or more resources by construction, then flattening the structure so obtained</skos:note>
	</owl:Class>
	
	<owl:Class rdf:about="&api4kp-ops;HorizontalKnowledgeProcessingTask">
		<rdfs:subClassOf rdf:resource="&api4kp-ops;SyntacticKnowledgeProcessingTask"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&api4kp-ops;involvesRole"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:intersectionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&api4kp-ops;Input">
							</rdf:Description>
							<owl:Restriction>
								<owl:onProperty rdf:resource="&api4kp-ops;isFilledBy"/>
								<owl:someValuesFrom rdf:resource="&api4kp;KnowledgeResource"/>
							</owl:Restriction>
						</owl:intersectionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&api4kp-ops;involvesRole"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:intersectionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&api4kp-ops;Product">
							</rdf:Description>
							<owl:Restriction>
								<owl:onProperty rdf:resource="&api4kp-ops;isFilledBy"/>
								<owl:someValuesFrom rdf:resource="&api4kp;KnowledgeResource"/>
							</owl:Restriction>
						</owl:intersectionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&api4kp-ops;retains"/>
				<owl:someValuesFrom rdf:resource="&api4kp;WorkOfKnowledge"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>horizontal knowledge processing task</rdfs:label>
		<skos:definition>syntactic processing task that consumes and produces resources at the same level of abstraction, altering the primary characteristic that is defined at level of abstraction (abstract syntax/lexicon, concrete syntax, encoding)</skos:definition>
