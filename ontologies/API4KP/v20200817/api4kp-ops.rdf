		<owl:imports rdf:resource="https://www.omg.org/spec/API4KP/api4kp-rel/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/API4KP/api4kp-series/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/API4KP/api4kp/"/>
