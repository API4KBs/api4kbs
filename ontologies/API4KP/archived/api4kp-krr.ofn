# Object Property: api4kp-krr:accordingTo (according to)

AnnotationAssertion(rdfs:label api4kp-krr:accordingTo "according to")
AnnotationAssertion(skos:definition api4kp-krr:accordingTo "relationship between a collection and an environment whereby the results of application of a mapping of the environment to a member of the collection are also members of the environment"^^xsd:string)
ObjectPropertyRange(api4kp-krr:accordingTo api4kp-krr:Environment)

# Object Property: api4kp-krr:allPreserve (all preserve)

AnnotationAssertion(rdfs:label api4kp-krr:allPreserve "all preserve")
ObjectPropertyDomain(api4kp-krr:allPreserve api4kp-krr:PreservingEnvironment)

# Object Property: api4kp-krr:hasEnvironmentMapping (has environment mapping)

AnnotationAssertion(rdfs:label api4kp-krr:hasEnvironmentMapping "has environment mapping")
InverseObjectProperties(api4kp-krr:hasEnvironmentMapping api4kp-krr:isEnvironmentMappingOf)
ObjectPropertyRange(api4kp-krr:hasEnvironmentMapping api4kp-krr:Mapping)

# Object Property: api4kp-krr:hasEnvironmentMember (has environment member)

AnnotationAssertion(rdfs:label api4kp-krr:hasEnvironmentMember "has environment member")
ObjectPropertyDomain(api4kp-krr:hasEnvironmentMember api4kp-krr:NonemptyEnvironment)
ObjectPropertyRange(api4kp-krr:hasEnvironmentMember owl:Thing)

# Object Property: api4kp-krr:hasFocusMember (has focus member)

AnnotationAssertion(rdfs:label api4kp-krr:hasFocusMember "has focus member")
SubObjectPropertyOf(api4kp-krr:hasFocusMember api4kp-krr:hasEnvironmentMember)

# Object Property: api4kp-krr:hasSetOfMappings (has set of mappings)

AnnotationAssertion(rdfs:label api4kp-krr:hasSetOfMappings "has set of mappings")

# Object Property: api4kp-krr:hasSnapshot (has snapshot)

AnnotationAssertion(rdfs:label api4kp-krr:hasSnapshot "has snapshot")
AnnotationAssertion(skos:definition api4kp-krr:hasSnapshot "relationship between a mutable source and its state at one timepoint."^^xsd:string)
ObjectPropertyDomain(api4kp-krr:hasSnapshot api4kp-krr:Mutable)
ObjectPropertyRange(api4kp-krr:hasSnapshot api4kp-krr:ImmutableEntity)

# Object Property: api4kp-krr:hasStructureType (has structure type)

AnnotationAssertion(rdfs:label api4kp-krr:hasStructureType "has structure type")

# Object Property: api4kp-krr:hasSubenvironment (has subenvironment)

AnnotationAssertion(rdfs:label api4kp-krr:hasSubenvironment "has subenvironment")
InverseObjectProperties(api4kp-krr:hasSubenvironment api4kp-krr:isSubenvironmentOf)

# Object Property: api4kp-krr:isEnvironmentMappingOf (is environment mapping of)

AnnotationAssertion(rdfs:label api4kp-krr:isEnvironmentMappingOf "is environment mapping of")

