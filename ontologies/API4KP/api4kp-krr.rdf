    <!-- https://www.omg.org/spec/API4KP/api4kp-krr/isSubenvironmentOf -->

    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/API4KP/api4kp-krr/isSubenvironmentOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://www.omg.org/spec/API4KP/api4kp-krr/Environment"/>
