    <!-- https://www.omg.org/spec/API4KP/api4kp-krr/accordingTo -->

    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/API4KP/api4kp-krr/accordingTo">
        <rdfs:range rdf:resource="https://www.omg.org/spec/API4KP/api4kp-krr/Environment"/>
        <rdfs:label>according to</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">relationship between a collection and an environment whereby the results of application of a mapping of the environment to a member of the collection are also members of the environment</skos:definition>
    </owl:ObjectProperty>
    


