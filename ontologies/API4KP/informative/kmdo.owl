############################
#   Object Properties
############################

# Object Property: kmo:archived-in (archived in)

AnnotationAssertion(rdfs:label kmo:archived-in "archived in")
SubObjectPropertyOf(kmo:archived-in api4kp:stored-in)
InverseObjectProperties(kmo:archived-in kmo:archives)
ObjectPropertyDomain(kmo:archived-in api4kp:KnowledgeArtifact)

# Object Property: kmo:archives (archives)

AnnotationAssertion(rdfs:comment kmo:archives "Archival is a kind of storage implying no immediate use. Archiving is scoped to artifacts only")
AnnotationAssertion(rdfs:label kmo:archives "archives")
SubObjectPropertyOf(kmo:archives api4kp:stores)
ObjectPropertyRange(kmo:archives api4kp:KnowledgeArtifact)

# Object Property: kmo:endorsed-by (endorsed by)

AnnotationAssertion(rdfs:label kmo:endorsed-by "endorsed by")
SubObjectPropertyOf(kmo:endorsed-by api4kp:internally-represented-by)
InverseObjectProperties(kmo:endorsed-by kmo:endorses)
ObjectPropertyDomain(kmo:endorsed-by api4kp:KnowledgeEndeavour)
ObjectPropertyRange(kmo:endorsed-by api4kp:KnowledgeableAgent)

# Object Property: kmo:endorses (endorses)

AnnotationAssertion(rdfs:label kmo:endorses "endorses")
AnnotationAssertion(skos:definition kmo:endorses "An agent's endorsement of a knowledge endeavor implies (i) that the agent is able to understand the content of that endeavor, internally representing the knowledge (expressed by (the expression carried)) in the endeavor, (ii) that the agent transfers their reputation and trust to that endeavor")
SubObjectPropertyOf(kmo:endorses api4kp:internally-represents)
ObjectPropertyDomain(kmo:endorses api4kp:KnowledgeableAgent)
ObjectPropertyRange(kmo:endorses api4kp:KnowledgeEndeavour)

# Object Property: kmo:focus (has focus)

AnnotationAssertion(rdfs:label kmo:focus "has focus")
SubObjectPropertyOf(kmo:focus dc-t:subject)

# Object Property: kmo:possessed-by ((knowledge) possessed by)

AnnotationAssertion(rdfs:label kmo:possessed-by "(knowledge) possessed by")
InverseObjectProperties(kmo:possessed-by kmo:possesses)
ObjectPropertyDomain(kmo:possessed-by api4kp:Knowledge)
ObjectPropertyRange(kmo:possessed-by api4kp:KnowledgeableAgent)

# Object Property: kmo:possesses (possesses (knowledge))

AnnotationAssertion(rdfs:label kmo:possesses "possesses (knowledge)")
AnnotationAssertion(skos:definition kmo:possesses "An Agent possesses some Knowledge if they can conceptualize that Knowledge, and act based on that Knowledge")
AnnotationAssertion(skos:note kmo:possesses "The possession of some Knowledge is the outcome of a Knowledge Acquisition Activity")
ObjectPropertyDomain(kmo:possesses api4kp:KnowledgeableAgent)
ObjectPropertyRange(kmo:possesses api4kp:Knowledge)

# Object Property: kmo:primary-subject (has primary subject)

AnnotationAssertion(rdfs:label kmo:primary-subject "has primary subject")
SubObjectPropertyOf(kmo:primary-subject dc-t:subject)

# Object Property: kmo:replica-of (replica of)

AnnotationAssertion(rdfs:comment kmo:replica-of "An artifact that resulted from the copy of a 'master copy' is a replica of that copy")
AnnotationAssertion(rdfs:label kmo:replica-of "replica of")
SubObjectPropertyOf(kmo:replica-of api4kp:copy-of)
ObjectPropertyDomain(kmo:replica-of api4kp:KnowledgeArtifact)
ObjectPropertyRange(kmo:replica-of kmo:MasterCopy)

# Object Property: kmo:secondary-subject (kmo:secondary-subject)

SubObjectPropertyOf(kmo:secondary-subject dc-t:subject)

# Object Property: dc-t:audience (has audience)

AnnotationAssertion(rdfs:label dc-t:audience "has audience")
SubObjectPropertyOf(dc-t:audience kmo:associated-with)

# Object Property: dc-t:subject (has subject)

AnnotationAssertion(rdfs:label dc-t:subject "has subject")
SubObjectPropertyOf(dc-t:subject api4kp:is-about)

# Object Property: rdf:type (is a)

AnnotationAssertion(rdfs:label rdf:type "is a")
SubObjectPropertyOf(rdf:type kmo:associated-with)

# Object Property: api4kp:captures (captures)

AnnotationAssertion(rdfs:label api4kp:captures "captures")
SubObjectPropertyOf(api4kp:captures api4kp:is-about)

# Object Property: api4kp:defined-in-terms-of (in terms of)

AnnotationAssertion(rdfs:label api4kp:defined-in-terms-of "in terms of")
SubObjectPropertyOf(api4kp:defined-in-terms-of api4kp:is-about)

# Object Property: api4kp:defines (defines)

AnnotationAssertion(rdfs:label api4kp:defines "defines")
SubObjectPropertyOf(api4kp:defines api4kp:is-about)

# Object Property: api4kp:identified-by (has ID)

AnnotationAssertion(rdfs:label api4kp:identified-by "has ID")
SubObjectPropertyOf(api4kp:identified-by kmo:associated-with)

# Object Property: api4kp:is-about (is about)

AnnotationAssertion(rdfs:label api4kp:is-about "is about")
SubObjectPropertyOf(api4kp:is-about kmo:associated-with)

# Object Property: api4kp:knowledge-representation-of (represents)

AnnotationAssertion(rdfs:label api4kp:knowledge-representation-of "represents")
SubObjectPropertyOf(api4kp:knowledge-representation-of api4kp:is-about)



############################
#   Classes
############################

# Class: kmo:AdvancedKnowledge (advanced knowledge)

AnnotationAssertion(rdfs:label kmo:AdvancedKnowledge "advanced knowledge"@en)
EquivalentClasses(kmo:AdvancedKnowledge ObjectIntersectionOf(api4kp:Knowledge ObjectComplementOf(kmo:CommonKnowledge)))
SubClassOf(kmo:AdvancedKnowledge api4kp:Knowledge)

# Class: kmo:AnonymousKnowledgeResource (anonymous knowledge resource)

AnnotationAssertion(rdfs:label kmo:AnonymousKnowledgeResource "anonymous knowledge resource"@en)
AnnotationAssertion(skos:definition kmo:AnonymousKnowledgeResource "Knowledge Resource that is not identified by a an ID assigned by a central identification authority")
EquivalentClasses(kmo:AnonymousKnowledgeResource ObjectIntersectionOf(api4kp:KnowledgeResource ObjectComplementOf(ObjectSomeValuesFrom(api4kp:identified-by api4kp:Identifier))))
SubClassOf(kmo:AnonymousKnowledgeResource api4kp:KnowledgeResource)
DisjointClasses(kmo:AnonymousKnowledgeResource kmo:IdentifiedKnowledgeResource)

# Class: kmo:ApproachableKnowledgeResource (approachable knowledge resource)

AnnotationAssertion(rdfs:label kmo:ApproachableKnowledgeResource "approachable knowledge resource"@en)
EquivalentClasses(kmo:ApproachableKnowledgeResource ObjectIntersectionOf(kmo:AvailableKnowledgeResource kmo:BroadlyUnderstandableKnowledgeResource))
SubClassOf(kmo:ApproachableKnowledgeResource api4kp:KnowledgeResource)

# Class: kmo:AvailableKnowledgeResource (available knowledge resource)

AnnotationAssertion(rdfs:label kmo:AvailableKnowledgeResource "available knowledge resource"@en)
AnnotationAssertion(skos:definition kmo:AvailableKnowledgeResource "There exists a repository where an agent is allowed to obtain a copy of (some artifact that carries) the knowledge resource")
AnnotationAssertion(skos:note kmo:AvailableKnowledgeResource "More specifically, an artifact that carries a representation of the expression of some (version of a) work of knowledge")
EquivalentClasses(kmo:AvailableKnowledgeResource ObjectIntersectionOf(api4kp:KnowledgeResource ObjectSomeValuesFrom(api4kp:carried-by api4kp:KnowledgeArtifact) ObjectSomeValuesFrom(api4kp:stored-in ObjectSomeValuesFrom(api4kp:has-function kmo:DigitalKnowledgeArtifactRepository))))
SubClassOf(kmo:AvailableKnowledgeResource api4kp:KnowledgeResource)

# Class: kmo:BodyofKnowledge (body of knowledge)

AnnotationAssertion(rdfs:label kmo:BodyofKnowledge "body of knowledge")
AnnotationAssertion(skos:definition kmo:BodyofKnowledge "A complex corpus that encompasses the entirety of the knowledge about a specific Business Domain")
SubClassOf(kmo:BodyofKnowledge kmo:DomainSpecificWorkOfKnowledge)

# Class: kmo:BroadlyUnderstandableKnowledgeResource (broadly understandable knowledge resource)

AnnotationAssertion(rdfs:label kmo:BroadlyUnderstandableKnowledgeResource "broadly understandable knowledge resource"@en)
AnnotationAssertion(skos:definition kmo:BroadlyUnderstandableKnowledgeResource "Knowledge Resource that is interpreted by an agent that possesses Common Knowledge, and can be interpreted by an agent even if Common Knowledge is all the Knowledge they possess")
AnnotationAssertion(skos:note kmo:BroadlyUnderstandableKnowledgeResource "\"Understandability' is defined at the concept level, so that an agent can (re)construct an internal representation of the knowledge, reconnecting the it to the related concepts that agent is expected to possess, and possibly make pragmatic use of it.
 
The availability of a carrier, and the expression of the knowledge in a language that can be parsed by the agent are necessary, but not sufficient conditions.")
SubClassOf(kmo:BroadlyUnderstandableKnowledgeResource ObjectIntersectionOf(api4kp:KnowledgeResource ObjectSomeValuesFrom(api4kp:internally-represented-by ObjectIntersectionOf(api4kp:KnowledgeableAgent ObjectSomeValuesFrom(kmo:possesses kmo:CommonKnowledge)))))

# Class: kmo:BusinessDomain (business domain)

AnnotationAssertion(rdfs:label kmo:BusinessDomain "business domain"@en)
AnnotationAssertion(skos:definition kmo:BusinessDomain "A concept that synthesizes, abstracting and implying it, a functional complex of more granular concepts that define all the entities and relationships (actors, roles, activities and processes, resources, information, responsibilities, etc...) pertaining to a field or discipline")
AnnotationAssertion(skos:example kmo:BusinessDomain "cooking")
AnnotationAssertion(skos:example kmo:BusinessDomain "finance")
AnnotationAssertion(skos:example kmo:BusinessDomain "healthcare")
SubClassOf(kmo:BusinessDomain api4kp:ComplexConcept)

# Class: kmo:CanonicalKnowledgeRepresentation (TODO canonical knowledge Representation - Reposition)

AnnotationAssertion(rdfs:label kmo:CanonicalKnowledgeRepresentation "TODO canonical knowledge Representation - Reposition"@en)
AnnotationAssertion(skos:definition kmo:CanonicalKnowledgeRepresentation "Knowledge Representation that 
(i) expresses a Work of Knowledge that captures no more and no less than an intended Piece of Knowledge
(ii) is expressed using the preferred language for the kind of work the resource represents
(iii) faithufully represents that Work of Knowledge")
SubClassOf(kmo:CanonicalKnowledgeRepresentation api4kp:KnowledgeResource)
SubClassOf(kmo:CanonicalKnowledgeRepresentation ObjectExactCardinality(1 api4kp:faithfully-expresses api4kp:WorkOfKnowledge))
SubClassOf(kmo:CanonicalKnowledgeRepresentation ObjectExactCardinality(1 api4kp:knowledge-representation-of api4kp:PieceOfKnowledge))

# Class: kmo:ClinicalBusinessDomain (clinical business domain)

AnnotationAssertion(rdfs:label kmo:ClinicalBusinessDomain "clinical business domain"@en)
AnnotationAssertion(skos:definition kmo:ClinicalBusinessDomain "A domain that includes concepts dealing with risk/cause, evaluation, diagnosis, treatment, prevention and management of health and wellness conditions, including methods for the appropriate application of tests, procedures, drugs, and treatments.")
AnnotationAssertion(skos:scopeNote kmo:ClinicalBusinessDomain "This is considered a class since there are multiple notions of 'Clinical Domain', varying e.g. by country or over time.")
SubClassOf(kmo:ClinicalBusinessDomain kmo:MedicalBusinessDomain)

# Class: kmo:ClinicalKnowledgeResource (clinical knowledge resource)

AnnotationAssertion(rdfs:label kmo:ClinicalKnowledgeResource "clinical knowledge resource"@en)
AnnotationAssertion(skos:definition kmo:ClinicalKnowledgeResource "Asset that is the realization of a Work of Knowledge, whose subject includes on one or more concepts from some Clinical Domain")
EquivalentClasses(kmo:ClinicalKnowledgeResource ObjectIntersectionOf(kmo:DomainSpecificKnowledgeResource ObjectSomeValuesFrom(api4kp:knowledge-representation-of ObjectIntersectionOf(kmo:DomainKnowledge ObjectSomeValuesFrom(api4kp:is-about kmo:ClinicalBusinessDomain)))))
SubClassOf(kmo:ClinicalKnowledgeResource kmo:DomainSpecificKnowledgeResource)

# Class: kmo:CommonKnowledge (common knowledge)

AnnotationAssertion(rdfs:label kmo:CommonKnowledge "common knowledge"@en)
AnnotationAssertion(skos:definition kmo:CommonKnowledge "Knowledge that everyone has, and everyone is known to have")
SubClassOf(kmo:CommonKnowledge api4kp:Knowledge)

# Class: kmo:ConcretizedPieceOfContent ((concretely realized) piece of content)

AnnotationAssertion(rdfs:label kmo:ConcretizedPieceOfContent "(concretely realized) piece of content"@en)
AnnotationAssertion(skos:altLabel kmo:ConcretizedPieceOfContent "PoC")
AnnotationAssertion(skos:definition kmo:ConcretizedPieceOfContent "Distributable, Concrete Expression of some Intellectual Work, in some Human Consumable Language")
SubClassOf(kmo:ConcretizedPieceOfContent kmo:DistributableExpression)
SubClassOf(kmo:ConcretizedPieceOfContent kmo:HumanConsumableExpression)

# Class: kmo:ConcretizedPieceOfKnowledge ((concretely realized) piece of knowledge)

AnnotationAssertion(rdfs:label kmo:ConcretizedPieceOfKnowledge "(concretely realized) piece of knowledge"@en)
AnnotationAssertion(skos:altLabel kmo:ConcretizedPieceOfKnowledge "PoK")
AnnotationAssertion(skos:definition kmo:ConcretizedPieceOfKnowledge "A Piece of Knowledge is a Distributable, Concrete Expression of an Intellectual Work of Knowledge")
SubClassOf(kmo:ConcretizedPieceOfKnowledge kmo:DistributableExpression)
SubClassOf(kmo:ConcretizedPieceOfKnowledge ObjectAllValuesFrom(api4kp:concretely-expresses api4kp:WorkOfKnowledge))

# Class: kmo:ConcretizedPieceOfKnowledgeContent ((concretely realized) piece of knowledge content)

AnnotationAssertion(rdfs:label kmo:ConcretizedPieceOfKnowledgeContent "(concretely realized) piece of knowledge content"@en)
AnnotationAssertion(skos:altLabel kmo:ConcretizedPieceOfKnowledgeContent "PoCK")
AnnotationAssertion(skos:definition kmo:ConcretizedPieceOfKnowledgeContent "Piece of Content that is the expression of a Work of knowledge")
AnnotationAssertion(skos:note kmo:ConcretizedPieceOfKnowledgeContent "Like any Piece of Content, a PoCK is expressed using some natural language, and formatted for consumption by a human audience (as opposed to be used for computation by a software)")
EquivalentClasses(kmo:ConcretizedPieceOfKnowledgeContent ObjectIntersectionOf(kmo:ConcretizedPieceOfContent kmo:ConcretizedPieceOfKnowledge))

# Class: kmo:CoreKnowledgeAsset (core knowledge asset)

AnnotationAssertion(rdfs:label kmo:CoreKnowledgeAsset "core knowledge asset"@en)
AnnotationAssertion(skos:definition kmo:CoreKnowledgeAsset "Asset such that Assets may be derived from it, but is not itself derived from any other Asset (although it could be a derivative of another Resource)")
EquivalentClasses(kmo:CoreKnowledgeAsset ObjectIntersectionOf(kmo:KnowledgeAsset ObjectIntersectionOf(kmo:CanonicalKnowledgeRepresentation kmo:EndorsedKnowledgeResource kmo:MainstreamKnowledgeResource)))
SubClassOf(kmo:CoreKnowledgeAsset kmo:KnowledgeAsset)
SubClassOf(kmo:CoreKnowledgeAsset ObjectComplementOf(ObjectSomeValuesFrom(api4kp:derived-from kmo:KnowledgeAsset)))

# Class: kmo:DigitalKnowledgeArtifact (digital knowledge artifact)

AnnotationAssertion(rdfs:label kmo:DigitalKnowledgeArtifact "digital knowledge artifact"@en)
AnnotationAssertion(skos:definition kmo:DigitalKnowledgeArtifact "An artifact that carries some form of knowledge encoded by means of discretized signals")
SubClassOf(kmo:DigitalKnowledgeArtifact api4kp:KnowledgeArtifact)

# Class: kmo:DigitalKnowledgeArtifactDirectory (digital knowledge artifact directory)

AnnotationAssertion(rdfs:label kmo:DigitalKnowledgeArtifactDirectory "digital knowledge artifact directory"@en)
AnnotationAssertion(skos:definition kmo:DigitalKnowledgeArtifactDirectory "Function of a (software) agent that relies on a Knowledge Artifact Inventory that references Digital Knowledge Artifacts")
SubClassOf(kmo:DigitalKnowledgeArtifactDirectory kmo:KnowledgeArtifactDirectory)

# Class: kmo:DigitalKnowledgeArtifactRepository (digital knowledge artifact repository)

AnnotationAssertion(rdfs:label kmo:DigitalKnowledgeArtifactRepository "digital knowledge artifact repository"@en)
AnnotationAssertion(skos:definition kmo:DigitalKnowledgeArtifactRepository "Function served by an entity that stores Digital Knowledge Artifacts")
SubClassOf(kmo:DigitalKnowledgeArtifactRepository api4kp:KnowledgeArtifactRepository)
SubClassOf(kmo:DigitalKnowledgeArtifactRepository ObjectSomeValuesFrom(api4kp:function-of ObjectSomeValuesFrom(api4kp:stores kmo:DigitalKnowledgeArtifact)))

# Class: kmo:DigitalKnowledgeAssetSurrogate (digital knowledge asset surrogate)

AnnotationAssertion(rdfs:label kmo:DigitalKnowledgeAssetSurrogate "digital knowledge asset surrogate"@en)
EquivalentClasses(kmo:DigitalKnowledgeAssetSurrogate ObjectIntersectionOf(kmo:DigitalKnowledgeArtifact api4kp:KnowledgeAssetSurrogate))
SubClassOf(kmo:DigitalKnowledgeAssetSurrogate kmo:DigitalKnowledgeArtifact)

# Class: kmo:DigitalKnowledgeAssetSurrogateRepository (digital knowledge asset surrogate repository)

AnnotationAssertion(rdfs:label kmo:DigitalKnowledgeAssetSurrogateRepository "digital knowledge asset surrogate repository"@en)
AnnotationAssertion(skos:definition kmo:DigitalKnowledgeAssetSurrogateRepository "a repository specialized in digital asset surrogates")
SubClassOf(kmo:DigitalKnowledgeAssetSurrogateRepository kmo:DigitalKnowledgeArtifactRepository)
SubClassOf(kmo:DigitalKnowledgeAssetSurrogateRepository ObjectSomeValuesFrom(api4kp:function-of ObjectSomeValuesFrom(api4kp:stores kmo:DigitalKnowledgeAssetSurrogate)))

# Class: kmo:DigitalKnowledgeManagementServiceProvider (digital knowledge management dervice provider)

AnnotationAssertion(rdfs:label kmo:DigitalKnowledgeManagementServiceProvider "digital knowledge management dervice provider"@en)
AnnotationAssertion(skos:definition kmo:DigitalKnowledgeManagementServiceProvider "knowledge management service provider for digital knowledge artifacts")
SubClassOf(kmo:DigitalKnowledgeManagementServiceProvider kmo:KnowledgeManagementServiceProvider)
SubClassOf(kmo:DigitalKnowledgeManagementServiceProvider ObjectSomeValuesFrom(api4kp:has-part kmo:DigitalKnowledgeArtifactDirectory))
SubClassOf(kmo:DigitalKnowledgeManagementServiceProvider ObjectSomeValuesFrom(api4kp:has-part kmo:DigitalKnowledgeArtifactRepository))
SubClassOf(kmo:DigitalKnowledgeManagementServiceProvider ObjectSomeValuesFrom(api4kp:has-part kmo:KnowledgeArtifactEditor))

# Class: kmo:DistributableExpression (distributable expression)

AnnotationAssertion(rdfs:label kmo:DistributableExpression "distributable expression"@en)
AnnotationAssertion(skos:definition kmo:DistributableExpression "Concrete Expression that is realized by a self-contained knowledge artifact, and suitable to be used in a knowledge transfer activity")
SubClassOf(kmo:DistributableExpression api4kp:ConcreteExpression)
