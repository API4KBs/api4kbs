############################
#   Object Properties
############################

# Object Property: kmo:archives (kmo:archives)

AnnotationAssertion(rdfs:comment kmo:archives "Archival is a kind of storage implying no immediate use")
SubObjectPropertyOf(kmo:archives kmo:stores)
ObjectPropertyRange(kmo:archives <http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeArtifact>)

# Object Property: kmo:endorses (endorses)

AnnotationAssertion(rdfs:label kmo:endorses "endorses")
AnnotationAssertion(skos:definition kmo:endorses "An agent's endorsement of a knowledge endeavor implies (i) that the agent is able to understand the content of that endeavor, internally representing the knowledge (expressed by (the expression carried)) in the endeavor, (ii) that the agent transfers their reputation and trust to that endeavor")
SubObjectPropertyOf(kmo:endorses <http://www.omg.org/spec/API4KP/core#internally-represents>)
ObjectPropertyDomain(kmo:endorses <http://www.omg.org/spec/API4KP/core#KnowledgeableAgent>)
ObjectPropertyRange(kmo:endorses <http://www.omg.org/spec/API4KP/core#KnowledgeEndeavour>)

# Object Property: kmo:function-of (function of)

AnnotationAssertion(rdfs:label kmo:function-of "function of")
InverseObjectProperties(kmo:function-of kmo:has-function)
ObjectPropertyDomain(kmo:function-of kmo:Function)

# Object Property: kmo:has-function (has function)

AnnotationAssertion(rdfs:label kmo:has-function "has function")
ObjectPropertyRange(kmo:has-function kmo:Function)

# Object Property: kmo:possessed-by ((knowledge) possessed by)

AnnotationAssertion(rdfs:label kmo:possessed-by "(knowledge) possessed by")
InverseObjectProperties(kmo:possessed-by kmo:possesses)
ObjectPropertyDomain(kmo:possessed-by <http://www.omg.org/spec/API4KP/core#Knowledge>)
ObjectPropertyRange(kmo:possessed-by <http://www.omg.org/spec/API4KP/core#KnowledgeableAgent>)

# Object Property: kmo:possesses (possesses (knowledge))

AnnotationAssertion(rdfs:label kmo:possesses "possesses (knowledge)")
AnnotationAssertion(skos:definition kmo:possesses "An Agent possesses some Knowledge if they can conceptualize that Knowledge, and act based on that Knowledge")
AnnotationAssertion(skos:note kmo:possesses "The possession of some Knowledge is the outcome of a Knowledge Acquisition Activity")
ObjectPropertyDomain(kmo:possesses <http://www.omg.org/spec/API4KP/core#KnowledgeableAgent>)
ObjectPropertyRange(kmo:possesses <http://www.omg.org/spec/API4KP/core#Knowledge>)

# Object Property: kmo:stored-by (stored by)

AnnotationAssertion(rdfs:comment kmo:stored-by "The relationship between a Work, Expression or Artifact, and entity that keeps it in storage.")
AnnotationAssertion(rdfs:label kmo:stored-by "stored by")
InverseObjectProperties(kmo:stored-by kmo:stores)
ObjectPropertyDomain(kmo:stored-by <http://www.omg.org/spec/API4KP/core#KnowledgeEndeavour>)

# Object Property: kmo:stores (stores)

AnnotationAssertion(rdfs:comment kmo:stores "An entity stores a Work, Expression or Artifact if it is able to keep (a copy of) it, and make it accessible for use as needed")
AnnotationAssertion(rdfs:label kmo:stores "stores")
AnnotationAssertion(skos:note kmo:stores "Notice that only Artifacts can be stored in practice. Storing a Work or an Expression implies the existence of an Artifact that carries (the Expression that realized) that Work")
ObjectPropertyRange(kmo:stores <http://www.omg.org/spec/API4KP/core#KnowledgeEndeavour>)



############################
#   Classes
############################

# Class: <http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeArtifact> (<http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeArtifact>)

AnnotationAssertion(skos:altLabel <http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeArtifact> "Knowledge Document")
AnnotationAssertion(skos:example <http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeArtifact> "A PDF Document carrying 1) a tutorial on OWL ontologies (in English) and 2) the RDF/XML serialization of an example ontology in appendix A")
AnnotationAssertion(skos:example <http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeArtifact> "An XML file that carries the XML serialization of a RuleML rulebase")

# Class: <http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeResource> (<http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeResource>)

AnnotationAssertion(skos:example <http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeResource> "The Ontology of Knowledge Management, expressed in OWL (and English as a sub-language)")
AnnotationAssertion(skos:example <http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeResource> "The mass-energy equation ('E=mc^2'), as expressed in some language for the representation of math knowledge")

# Class: <http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeResourceDescription> (Knowledge Asset Descriptor)

AnnotationAssertion(rdfs:label <http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeResourceDescription> "Knowledge Asset Descriptor"@en)
AnnotationAssertion(skos:definition <http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeResourceDescription> "a Knowledge Asset that has another Asset as subject")
EquivalentClasses(<http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeResourceDescription> ObjectIntersectionOf(kmo:KnowledgeAsset ObjectIntersectionOf(<http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeResourceDescription> ObjectAllValuesFrom(<http://purl.org/vocab/frbr/core#subject> kmo:KnowledgeAsset))))
SubClassOf(<http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeResourceDescription> <http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeResourceDescription>)
SubClassOf(<http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeResourceDescription> kmo:KnowledgeAsset)

# Class: kmo:Activity (kmo:Activity)

AnnotationAssertion(skos:note kmo:Activity "(placeholder for compatibility with ontologies of activities and processes)")

# Class: kmo:AdvancedKnowledge (advanced knowledge)

AnnotationAssertion(rdfs:label kmo:AdvancedKnowledge "advanced knowledge"@en)
EquivalentClasses(kmo:AdvancedKnowledge ObjectIntersectionOf(<http://www.omg.org/spec/API4KP/core#Knowledge> ObjectComplementOf(kmo:CommonKnowledge)))
SubClassOf(kmo:AdvancedKnowledge <http://www.omg.org/spec/API4KP/core#Knowledge>)

# Class: kmo:AnonymousKnowledgeResource (Anonymous Knowledge Resource)

AnnotationAssertion(rdfs:label kmo:AnonymousKnowledgeResource "Anonymous Knowledge Resource"@en)
AnnotationAssertion(skos:definition kmo:AnonymousKnowledgeResource "Knowledge Resource that is not identified by a an ID assigned by a central identification authority")
EquivalentClasses(kmo:AnonymousKnowledgeResource ObjectIntersectionOf(<http://www.omg.org/spec/API4KP/core#KnowledgeResource> ObjectComplementOf(ObjectSomeValuesFrom(kmo:identified-by <http://www.omg.org/spec/API4KP/core#Identifier>))))
SubClassOf(kmo:AnonymousKnowledgeResource <http://www.omg.org/spec/API4KP/core#KnowledgeResource>)
DisjointClasses(kmo:AnonymousKnowledgeResource kmo:IdentifiedKnowledgeResource)

# Class: kmo:ApproachableKnowledgeResource (Approachable Knowledge Resource)

AnnotationAssertion(rdfs:label kmo:ApproachableKnowledgeResource "Approachable Knowledge Resource"@en)
EquivalentClasses(kmo:ApproachableKnowledgeResource ObjectIntersectionOf(kmo:AvailableKnowledgeResource kmo:BroadlyUnderstandableKnowledgeResource))
SubClassOf(kmo:ApproachableKnowledgeResource <http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeResource>)

# Class: kmo:AvailableKnowledgeResource (Available Knowledge Resource)

AnnotationAssertion(rdfs:label kmo:AvailableKnowledgeResource "Available Knowledge Resource"@en)
AnnotationAssertion(skos:definition kmo:AvailableKnowledgeResource "There exists a repository where an agent is allowed to obtain a copy of an artifact that carries the knowledge resource")
EquivalentClasses(kmo:AvailableKnowledgeResource ObjectIntersectionOf(<http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeResource> ObjectSomeValuesFrom(kmo:serialized-into <http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeArtifact>) ObjectSomeValuesFrom(kmo:stored-by ObjectSomeValuesFrom(kmo:has-function kmo:DigitalKnowledgeArtifactRepository))))
SubClassOf(kmo:AvailableKnowledgeResource <http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeResource>)

# Class: kmo:BodyofKnowledge (body of knowledge)

AnnotationAssertion(rdfs:label kmo:BodyofKnowledge "body of knowledge")
AnnotationAssertion(skos:definition kmo:BodyofKnowledge "A complex corpus that encompasses the entirety of the knowledge about a specific Business Domain")
SubClassOf(kmo:BodyofKnowledge kmo:DomainSpecificWorkOfKnowledge)

# Class: kmo:BroadlyUnderstandableKnowledgeResource (Broadly Understandable Knowledge Resource)

AnnotationAssertion(rdfs:label kmo:BroadlyUnderstandableKnowledgeResource "Broadly Understandable Knowledge Resource"@en)
AnnotationAssertion(skos:definition kmo:BroadlyUnderstandableKnowledgeResource "Broadly Understandable Knowledge Resource: Knowledge Resource that can possibly be interpreted even by an agent that only possesses Common Knowledge.")
EquivalentClasses(kmo:BroadlyUnderstandableKnowledgeResource ObjectIntersectionOf(<http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeResource> ObjectSomeValuesFrom(kmo:possessed-by ObjectIntersectionOf(<http://www.omg.org/spec/API4KP/FRBR-KR#literate-agent> ObjectComplementOf(ObjectSomeValuesFrom(kmo:possesses kmo:DomainKnowledge)) ObjectSomeValuesFrom(kmo:possesses kmo:CommonKnowledge)))))
SubClassOf(kmo:BroadlyUnderstandableKnowledgeResource <http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeResource>)

# Class: kmo:BusinessDomain (business domain)

AnnotationAssertion(rdfs:label kmo:BusinessDomain "business domain"@en)
AnnotationAssertion(skos:definition kmo:BusinessDomain "A concept that synthesizes, abstracting and implying it, a functional complex of more granular concepts that define all the entities and relationships (actors, roles, activities and processes, resources, information, responsibilities, etc...) pertaining to a field or discipline")
AnnotationAssertion(skos:example kmo:BusinessDomain "cooking")
AnnotationAssertion(skos:example kmo:BusinessDomain "finance")
AnnotationAssertion(skos:example kmo:BusinessDomain "healthcare")
SubClassOf(kmo:BusinessDomain <http://www.omg.org/spec/API4KP/core#ComplexConcept>)

# Class: kmo:CanonicalKnowledgeResource (Canonical Knowledge Resource)

AnnotationAssertion(rdfs:label kmo:CanonicalKnowledgeResource "Canonical Knowledge Resource"@en)
AnnotationAssertion(skos:definition kmo:CanonicalKnowledgeResource "Knowledge Resource that is expressed using the preferred language for the kind of work the resource represents")
SubClassOf(kmo:CanonicalKnowledgeResource <http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeResource>)

# Class: kmo:ClinicalBusinessDomain (clinical business domain)

AnnotationAssertion(rdfs:label kmo:ClinicalBusinessDomain "clinical business domain"@en)
AnnotationAssertion(skos:definition kmo:ClinicalBusinessDomain "A domain the includes concepts dealing with risk/cause, evaluation, diagnosis, treatment, prevention and management of health and wellness conditions, including methods for the appropriate application of tests, procedures, drugs, and treatments.")
AnnotationAssertion(skos:scopeNote kmo:ClinicalBusinessDomain "This is considered a class since there are multiple notions of 'Clinical Domain', varying e.g. by country or over time.")
SubClassOf(kmo:ClinicalBusinessDomain kmo:MedicalBusinessDomain)

# Class: kmo:ClinicalKnowledgeResource (Clinical Knowledge Resource)

AnnotationAssertion(rdfs:label kmo:ClinicalKnowledgeResource "Clinical Knowledge Resource"@en)
AnnotationAssertion(skos:definition kmo:ClinicalKnowledgeResource "Asset that is the realization of a Work of Knowledge, whose subject includes on one or more concepts from some Clinical Domain")
SubClassOf(kmo:ClinicalKnowledgeResource kmo:DomainSpecificKnowledgeResource)

# Class: kmo:CommonKnowledge (common knowledge)

AnnotationAssertion(rdfs:label kmo:CommonKnowledge "common knowledge"@en)
AnnotationAssertion(skos:definition kmo:CommonKnowledge "Knowledge that everyone has, and everyone is known to have")
SubClassOf(kmo:CommonKnowledge <http://www.omg.org/spec/API4KP/core#Knowledge>)

# Class: kmo:ConcretizedPieceOfContent ((concretely realized) piece of content)

AnnotationAssertion(rdfs:label kmo:ConcretizedPieceOfContent "(concretely realized) piece of content"@en)
AnnotationAssertion(skos:altLabel kmo:ConcretizedPieceOfContent "PoC")
AnnotationAssertion(skos:definition kmo:ConcretizedPieceOfContent "Distributable, Concrete Expression of some Intellectual Work, in some Human Consumable Language")
SubClassOf(kmo:ConcretizedPieceOfContent kmo:DistributableExpression)
SubClassOf(kmo:ConcretizedPieceOfContent kmo:HumanConsumableExpression)

# Class: kmo:ConcretizedPieceOfKnowledge ((concretely realized) piece of knowledge)

AnnotationAssertion(rdfs:label kmo:ConcretizedPieceOfKnowledge "(concretely realized) piece of knowledge"@en)
AnnotationAssertion(skos:altLabel kmo:ConcretizedPieceOfKnowledge "PoK")
AnnotationAssertion(skos:definition kmo:ConcretizedPieceOfKnowledge "A Piece of Knowledge is a Distributable, Concrete Expression of an Intellectual Work of Knowledge")
SubClassOf(kmo:ConcretizedPieceOfKnowledge kmo:DistributableExpression)
SubClassOf(kmo:ConcretizedPieceOfKnowledge ObjectAllValuesFrom(<http://www.omg.org/spec/API4KP/core#concretely-expresses> <http://www.omg.org/spec/API4KP/core#WorkOfKnowledge>))

# Class: kmo:ConcretizedPieceOfKnowledgeContent ((concretely realized) piece of knowledge content)

AnnotationAssertion(rdfs:label kmo:ConcretizedPieceOfKnowledgeContent "(concretely realized) piece of knowledge content"@en)
AnnotationAssertion(skos:altLabel kmo:ConcretizedPieceOfKnowledgeContent "PoCK")
AnnotationAssertion(skos:definition kmo:ConcretizedPieceOfKnowledgeContent "Piece of Content that is the expression of a Work of knowledge")
AnnotationAssertion(skos:note kmo:ConcretizedPieceOfKnowledgeContent "Like any Piece of Content, a PoCK is expressed using some natural language, and formatted for consumption by a human audience (as opposed to be used for computation by a software)")
EquivalentClasses(kmo:ConcretizedPieceOfKnowledgeContent ObjectIntersectionOf(kmo:ConcretizedPieceOfContent kmo:ConcretizedPieceOfKnowledge))

# Class: kmo:CoreKnowledgeAsset (Core Knowledge Asset)

AnnotationAssertion(rdfs:label kmo:CoreKnowledgeAsset "Core Knowledge Asset"@en)
AnnotationAssertion(skos:definition kmo:CoreKnowledgeAsset "Asset such that Assets may be derived from it, but is not itself derived from any other Asset (although it could be a derivative of another Resource)")
EquivalentClasses(kmo:CoreKnowledgeAsset ObjectIntersectionOf(kmo:KnowledgeAsset ObjectIntersectionOf(kmo:CanonicalKnowledgeResource kmo:EndorsedKnowledgeResource kmo:MainstreamKnowledgeResource)))
SubClassOf(kmo:CoreKnowledgeAsset kmo:KnowledgeAsset)
SubClassOf(kmo:CoreKnowledgeAsset ObjectComplementOf(ObjectSomeValuesFrom(prov:wasDerivedFrom kmo:KnowledgeAsset)))

# Class: kmo:DigitalKnowledgeArtifact (digital knowledge artifact)

AnnotationAssertion(rdfs:label kmo:DigitalKnowledgeArtifact "digital knowledge artifact"@en)
AnnotationAssertion(skos:definition kmo:DigitalKnowledgeArtifact "A digital (i.e. of electronic nature) artifact that carries some form of knowledge.")
SubClassOf(kmo:DigitalKnowledgeArtifact <http://www.omg.org/spec/API4KP/core#KnowledgeArtifact>)

# Class: kmo:DigitalKnowledgeArtifactDirectory (Digital Knowledge Artifact Directory)

AnnotationAssertion(rdfs:label kmo:DigitalKnowledgeArtifactDirectory "Digital Knowledge Artifact Directory"@en)
AnnotationAssertion(skos:definition kmo:DigitalKnowledgeArtifactDirectory "Function of a software that uses a Knowledge Artifact Inventory that references Digital Knowledge Artifacts")
SubClassOf(kmo:DigitalKnowledgeArtifactDirectory kmo:KnowledgeArtifactDirectory)
SubClassOf(kmo:DigitalKnowledgeArtifactDirectory ObjectSomeValuesFrom(<http://www.omg.org/spec/API4KP/core#has-part> ObjectIntersectionOf(kmo:KnowledgeArtifactInventory ObjectAllValuesFrom(kmo:identifies kmo:DigitalKnowledgeArtifact))))

# Class: kmo:DigitalKnowledgeArtifactEditor (Digital Knowledge Artifact Editor)

AnnotationAssertion(rdfs:label kmo:DigitalKnowledgeArtifactEditor "Digital Knowledge Artifact Editor"@en)
SubClassOf(kmo:DigitalKnowledgeArtifactEditor kmo:Function)

# Class: kmo:DigitalKnowledgeArtifactRepository (Digital Knowledge Artifact Repository)

AnnotationAssertion(rdfs:label kmo:DigitalKnowledgeArtifactRepository "Digital Knowledge Artifact Repository"@en)
AnnotationAssertion(skos:definition kmo:DigitalKnowledgeArtifactRepository "Function served by an entity that stores Digital Knowledge Artifacts")
SubClassOf(kmo:DigitalKnowledgeArtifactRepository kmo:KnowledgeArtifactRepository)
SubClassOf(kmo:DigitalKnowledgeArtifactRepository ObjectSomeValuesFrom(kmo:function-of ObjectSomeValuesFrom(kmo:stores kmo:DigitalKnowledgeArtifact)))

# Class: kmo:DigitalKnowledgeAssetSurrogate (digital knowledge asset surrogate)

AnnotationAssertion(rdfs:label kmo:DigitalKnowledgeAssetSurrogate "digital knowledge asset surrogate"@en)
EquivalentClasses(kmo:DigitalKnowledgeAssetSurrogate ObjectIntersectionOf(kmo:DigitalKnowledgeArtifact <http://www.omg.org/spec/API4KP/core#KnowledgeAssetSurrogate>))
SubClassOf(kmo:DigitalKnowledgeAssetSurrogate kmo:DigitalKnowledgeArtifact)

# Class: kmo:DigitalKnowledgeAssetSurrogateRepository (Digital Knowledge Asset Surrogate Repository)

AnnotationAssertion(rdfs:label kmo:DigitalKnowledgeAssetSurrogateRepository "Digital Knowledge Asset Surrogate Repository"@en)
SubClassOf(kmo:DigitalKnowledgeAssetSurrogateRepository kmo:DigitalKnowledgeArtifactRepository)
SubClassOf(kmo:DigitalKnowledgeAssetSurrogateRepository ObjectSomeValuesFrom(kmo:stores kmo:DigitalKnowledgeAssetSurrogate))

# Class: kmo:DigitalKnowledgeManagementServiceProvider (Digital Knowledge Management Service Provider)

AnnotationAssertion(rdfs:label kmo:DigitalKnowledgeManagementServiceProvider "Digital Knowledge Management Service Provider"@en)
AnnotationAssertion(skos:definition kmo:DigitalKnowledgeManagementServiceProvider "Function of a software that enables the creation, maintenance, storage, cataloguing, delivery/distribution of digital knowledge artifact")
SubClassOf(kmo:DigitalKnowledgeManagementServiceProvider kmo:KnowledgeManagementServiceProvider)
SubClassOf(kmo:DigitalKnowledgeManagementServiceProvider ObjectSomeValuesFrom(<http://www.omg.org/spec/API4KP/core#has-part> kmo:DigitalKnowledgeArtifactDirectory))
SubClassOf(kmo:DigitalKnowledgeManagementServiceProvider ObjectSomeValuesFrom(<http://www.omg.org/spec/API4KP/core#has-part> kmo:DigitalKnowledgeArtifactEditor))
SubClassOf(kmo:DigitalKnowledgeManagementServiceProvider ObjectSomeValuesFrom(<http://www.omg.org/spec/API4KP/core#has-part> kmo:DigitalKnowledgeArtifactRepository))

# Class: kmo:DistributableExpression (distributable expression)

AnnotationAssertion(rdfs:label kmo:DistributableExpression "distributable expression"@en)
AnnotationAssertion(skos:definition kmo:DistributableExpression "Concrete Expression that is realized by a self-contained knowledge artifact, and suitable to be used in a knowledge transfer activity")
SubClassOf(kmo:DistributableExpression <http://www.omg.org/spec/API4KP/core#ConcreteExpression>)
SubClassOf(kmo:DistributableExpression ObjectSomeValuesFrom(<http://www.omg.org/spec/API4KP/core#carried-by> <http://www.omg.org/spec/API4KP/core#SelfContainedKnowledgeArtifact>))
SubClassOf(kmo:DistributableExpression ObjectSomeValuesFrom(<http://www.omg.org/spec/API4KP/core#involved-in> kmo:KnowledgeTransferActivity))

# Class: kmo:DomainKnowledge (domain knowledge)

AnnotationAssertion(rdfs:label kmo:DomainKnowledge "domain knowledge"@en)
AnnotationAssertion(skos:definition kmo:DomainKnowledge "Knowledge specifically about, and thus scoped by, some Business Domain (e.g. healthcare)")
SubClassOf(kmo:DomainKnowledge ObjectIntersectionOf(kmo:ExplicitKnowledge ObjectSomeValuesFrom(<http://www.omg.org/spec/API4KP/core#is-about> kmo:BusinessDomain)))

# Class: kmo:DomainSpecificKnowledgeResource (Domain-Specific Knowledge Resource)

AnnotationAssertion(rdfs:label kmo:DomainSpecificKnowledgeResource "Domain-Specific Knowledge Resource"@en)
SubClassOf(kmo:DomainSpecificKnowledgeResource <http://www.omg.org/spec/API4KP/FRBR-KR#KnowledgeResource>)

# Class: kmo:DomainSpecificWorkOfKnowledge (domain-specific work of knowledge)

