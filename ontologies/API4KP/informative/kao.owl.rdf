<?xml version="1.0"?>
<rdf:RDF xmlns="http://ckm.ontology.mayo.edu/ontology/KAO#"
     xml:base="http://ckm.ontology.mayo.edu/ontology/KAO"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:ccg="http://ckm.ontology.mayo.edu/ontology/CCG#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:Aspects="http://www.omg.org/spec/API4KP/Aspects/"
     xmlns:kao="http://ckm.ontology.mayo.edu/ontology/KAO#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
