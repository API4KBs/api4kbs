     -->

    


    <!-- http://ontology.mayo.edu/ontologies/20180601/KMDO/DomainSpecificWorkOfKnowledge -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontologies/20180601/KMDO/DomainSpecificWorkOfKnowledge"/>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/AnnotationLanguage -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/AnnotationLanguage">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/DedicatedLanguage"/>
        <rdfs:label>annotation language</rdfs:label>
        <skos:definition>language used to express &quot;meta&quot; knowledge</skos:definition>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/AssessmentModel -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/AssessmentModel">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/InferentialAsset"/>
        <rdfs:comment>knowledge that allows to infer an otherwise unknown or not directly observable state or situation</rdfs:comment>
        <rdfs:label>Assessment Model</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/BusinessRuleAsset -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/BusinessRuleAsset">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/CategorizedKnowledgeAsset"/>
        <rdfs:comment>Assets in this category describe or specify how to limit an agent&apos;s degree of freedom, to prevent undesirable behaviors, in favor of more desirable ones.</rdfs:comment>
        <rdfs:comment>At its essence, knowledge of this kind is expressed using &quot;IF..DO..&quot; rules, usually by means of constraint-oriented languages with ECA and/or deontic aspects</rdfs:comment>
        <rdfs:label>Rule-Oriented Knowledge Asset</rdfs:label>
        <skos:definition>Knowledge about the social, organizational and behavioral aspects of how business is (not) required, expected or permitted to be conducted, as determined by some party that has jurisdiction over an environment.</skos:definition>
        <skos:prefLabel>Rules, Policies and Guidelines</skos:prefLabel>
        <skos:scopeNote>Business rules are deontic in nature, whereas alethic rules are considered inferential models.</skos:scopeNote>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/CareProcessModel -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/CareProcessModel">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/ClinicalKnowledgeAsset"/>
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/ProcessAsset"/>
        <rdfs:comment>Adaptive specification of how to perform (complex) clinical activities, properly orchestrated over time, so to achieve certain objectives and goals</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontology.mayo.edu/ontology/KAO"/>
        <rdfs:label xml:lang="en">Care Process Model</rdfs:label>
        <skos:altLabel xml:lang="en">CPM</skos:altLabel>
        <skos:example xml:lang="en">The Mayo Clinic Rectal Adenocarcinoma CPM</skos:example>
        <skos:scopeNote xml:lang="en">Includes AME Care Process Models (CPM)</skos:scopeNote>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://ontology.mayo.edu/ontology/KAO/CareProcessModel"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#example"/>
        <owl:annotatedTarget xml:lang="en">The Mayo Clinic Rectal Adenocarcinoma CPM</owl:annotatedTarget>
        <rdfs:seeAlso rdf:resource="https://clinicalknowledgemanagement.mayo.edu/topic/clinical-answers/CNT-20141231/CPM-20211573"/>
    </owl:Axiom>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/CategorizedKnowledgeAsset -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/CategorizedKnowledgeAsset">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp/KnowledgeAsset"/>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/ClinicalKnowledgeAsset -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/ClinicalKnowledgeAsset">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/DomainSpecificKnowledgeAsset"/>
        <rdfs:label>clinical knowledge asset</rdfs:label>
        <skos:definition>knowledge asset that uses primarily concepts from the clinical business domain</skos:definition>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/ClinicalPracticeGuideline -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/ClinicalPracticeGuideline">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/BusinessRuleAsset"/>
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/ClinicalKnowledgeAsset"/>
        <rdfs:comment xml:lang="en">Systematically developed statements to assist provider decisions about appropriate healthcare for specific clinical situations</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontology.mayo.edu/ontology/KAO"/>
        <rdfs:label xml:lang="en">Clinical Practice Guideline</rdfs:label>
        <skos:altLabel xml:lang="en">Clinical Practice Guideline</skos:altLabel>
        <skos:example xml:lang="en">The Mayo Clinic Sternotomy Guideline</skos:example>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://ontology.mayo.edu/ontology/KAO/ClinicalPracticeGuideline"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#example"/>
        <owl:annotatedTarget xml:lang="en">The Mayo Clinic Sternotomy Guideline</owl:annotatedTarget>
        <rdfs:seeAlso rdf:resource="http://mayoweb.mayo.edu/pmr/documents/PracticeGuidelineTemplateSternotomy.pdf"/>
    </owl:Axiom>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/ClinicalRule -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/ClinicalRule">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/BusinessRuleAsset"/>
        <rdfs:comment xml:lang="en">Computational logic to direct clinical interventions that is typically expressed as if/then statements but may also be represented by a diagram, decision table or other representation language</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontology.mayo.edu/ontology/KAO"/>
        <rdfs:label xml:lang="en">Clinical Business Rule</rdfs:label>
        <skos:altLabel xml:lang="en">CDS Rule</skos:altLabel>
        <skos:example xml:lang="en">The &apos;HGB value prior to RBC transfusion&apos; Recommendation Rule</skos:example>
        <skos:prefLabel xml:lang="en">Clinical Rule</skos:prefLabel>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://ontology.mayo.edu/ontology/KAO/ClinicalRule"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#example"/>
        <owl:annotatedTarget xml:lang="en">The &apos;HGB value prior to RBC transfusion&apos; Recommendation Rule</owl:annotatedTarget>
        <rdfs:seeAlso rdf:resource="https://clinicalknowledgemanagement.mayo.edu/mayo-clinic-rules-catalog/asset/km-asset/KAT-20323041"/>
    </owl:Axiom>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/CognitiveCareProcessModel -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/CognitiveCareProcessModel">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/CareProcessModel"/>
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/CognitiveProcessModel"/>
        <rdfs:comment>Knowledge whose primary subject is the cognitive aspects of a Care Process</rdfs:comment>
        <rdfs:label>Cognitive Care Process Model</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/CognitiveProcessModel -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/CognitiveProcessModel">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/ProcessAsset"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.omg.org/spec/API4KP/api4kp/has-part"/>
                <owl:someValuesFrom rdf:resource="http://ontology.mayo.edu/ontology/KAO/DecisionModel"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Knowledge Asset that captures how information is processed internally by an intelligent agent</rdfs:comment>
        <rdfs:label>Cognitive Process Model</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/CohortDefinition -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/CohortDefinition">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/InferentialAsset"/>
        <rdfs:comment>Definition used to classify persons into classes defined by some common characteristic</rdfs:comment>
        <rdfs:label>Cohort Definition</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/ComponentKnowledgeAsset -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/ComponentKnowledgeAsset">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/ExplicitRoleKnowledgeAsset"/>
        <rdfs:label>Component Knowledge Asset</rdfs:label>
        <skos:definition>Knowledge Asset that is part of some other (Composite) Knowledge Asset. The component may play a specific role within the broader Composite Asset</skos:definition>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/CompositeKnowledgeAsset -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/CompositeKnowledgeAsset">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/ExplicitRoleKnowledgeAsset"/>
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp/CompositeKnowledgeResource"/>
        <rdfs:label>Composite Knowledge Asset</rdfs:label>
        <skos:definition>Knowledge Asset that has part some other Knowledge Asset</skos:definition>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/ComputableDecisionModel -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/ComputableDecisionModel">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/DecisionModel"/>
        <rdfs:comment>Decision Model that includes an explicit specification of its decision making logic, suitable to be faithfully expressed in a computable language</rdfs:comment>
        <rdfs:label>Computable Decision Model</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/ComputationalTechnique -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/ComputationalTechnique">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp/KnowledgeProcessingTechnique"/>
        <owl:disjointWith rdf:resource="http://ontology.mayo.edu/ontology/KAO/NaturalisticTechnique"/>
        <rdfs:label>computational technique</rdfs:label>
        <skos:definition>Technique used in some knowledge processing task method specification that can be faithfully expressed in some machine-computable language</skos:definition>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/ConceptualModel -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/ConceptualModel">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/SemanticAsset"/>
        <rdfs:comment>Abstraction of the structure and/or behavior of the entities in a given domain</rdfs:comment>
        <rdfs:label>Conceptual (Domain) Model</rdfs:label>
        <skos:altLabel>domain conceptualization</skos:altLabel>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/ContextualizabledKnowledge -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/ContextualizabledKnowledge">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/ConceptualModel"/>
        <rdfs:comment>Conceptual Work of Knowledge that has been adapted for a given context</rdfs:comment>
        <rdfs:label>Contextualizable Knowledge</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/DecisionMakingTask -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/DecisionMakingTask">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp/InformationProcessingTask"/>
        <rdfs:label>decision making task</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/DecisionModel -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/DecisionModel">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/AssessmentModel"/>
        <rdfs:comment>Assessment that can be abstracted by means of a question, which requires to select one out of a set of admissible options</rdfs:comment>
        <rdfs:label>Decision Model</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/DecisionTaskModel -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/DecisionTaskModel">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/CognitiveProcessModel"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.omg.org/spec/API4KP/api4kp/defines"/>
                <owl:someValuesFrom rdf:resource="http://ontology.mayo.edu/ontology/KAO/DecisionMakingTask"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.omg.org/spec/API4KP/api4kp/part-of"/>
                <owl:someValuesFrom rdf:resource="http://ontology.mayo.edu/ontology/KAO/CognitiveProcessModel"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Model of who, when, where and why a Decision is made, usually as part of a broader process. Also known as Decision Making Process Model, usually includes a Decision Model that specifies how to actually choose the optimal option</rdfs:comment>
        <rdfs:label>Decision Task Model</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/DedicatedLanguage -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/DedicatedLanguage">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp/ConstructedLanguage"/>
        <rdfs:label>Dedicated Language</rdfs:label>
        <skos:definition>Language that has been designed specifically, and thus can be used, to faithfully express certain categories of works of knowledge, i.e. language that is known to provide constructs for some of the concepts that are expected (implied) by the type of knowledge being expressed</skos:definition>
        <skos:editorialNote>Need a better term than &quot;Dedicated&quot;?
&quot;Committed&quot; is an option, but seems too strong, e.g. see Ontologically Committed Language</skos:editorialNote>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/DocumentationTemplate -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/DocumentationTemplate">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/InformationAsset"/>
        <rdfs:comment>An organized collection of determinable concepts, possibly associated to questions, which are meant to be resolved in a particular context</rdfs:comment>
        <rdfs:label>Documentation Template</rdfs:label>
        <skos:scopeNote>more general than questionnaires, whose questions are meant to be answered by a human agent (possibly with computer aid), documentation templates can be used  by software agents as well</skos:scopeNote>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/DomainSpecificKnowledgeAsset -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/DomainSpecificKnowledgeAsset">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontologies/20180601/KMDO/DomainSpecificWorkOfKnowledge"/>
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp/KnowledgeAsset"/>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/Equation -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/Equation">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/FunctionalExpression"/>
        <rdfs:comment>A functional expression, defined in terms of two functional expressions connected by means of the well known equality operator.</rdfs:comment>
        <rdfs:label>Equation</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/ExplicitRoleKnowledgeAsset -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/ExplicitRoleKnowledgeAsset">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp/KnowledgeAsset"/>
        <rdfs:comment>Asset that is necessarily related to some entity, which has a specific type itself</rdfs:comment>
        <rdfs:label>Explicit Role Knowledge Asset</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/ExpressionLanguage -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/ExpressionLanguage">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/DedicatedLanguage"/>
        <rdfs:label>expression language</rdfs:label>
        <skos:definition>language used to express functional derivations</skos:definition>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/FactualKnowledg -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/FactualKnowledg">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/ConceptualModel"/>
        <rdfs:comment>Facts are suitable to be asserted</rdfs:comment>
        <rdfs:comment>Knowledge that is held to be a faithful conceptualization of the state of affairs in a given domain by all agents that share (and understand) it.</rdfs:comment>
        <rdfs:label>Factual Knowledge</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/FormalOntology -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/FormalOntology">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/ConceptualModel"/>
        <rdfs:comment>conceptual model that is based on formal principles, such as that allow to make inferences and/or verify the consistency of the model</rdfs:comment>
        <rdfs:label>Formal Ontology</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/FunctionalExpression -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/FunctionalExpression">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/AssessmentModel"/>
        <rdfs:comment>Knowledge about the application of well-known information processing methods, possibly chained together, which are stateless, functional and idempotent in nature.</rdfs:comment>
        <rdfs:label>Functional Expression</rdfs:label>
        <skos:example>&apos;mass&apos; times the square of the speed of light</skos:example>
    </owl:Class>
    


    <!-- http://ontology.mayo.edu/ontology/KAO/InferentialAsset -->

    <owl:Class rdf:about="http://ontology.mayo.edu/ontology/KAO/InferentialAsset">
        <rdfs:subClassOf rdf:resource="http://ontology.mayo.edu/ontology/KAO/CategorizedKnowledgeAsset"/>
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp/Definition"/>
        <rdfs:comment>When qualitiative, knowledge of this kind can be generally abstracted by means of &quot;if..then..&quot; rules (or analogous formalisms). The quantitative counterpart is expressed by means of mathematical formulas. 

Either way, expressions are expected to be pure, functional and idempotent, even when they handle uncertainty.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontology.mayo.edu/ontology/KAO"/>
        <rdfs:label xml:lang="en">Inference-Oriented Knowledge Asset</rdfs:label>
        <skos:definition>Knowledge about how to assess the current, or predict the future state of a variable, quality or other characteristic of a system (e.g. a patient) which may not be directly observable, but can be estimated on the basis of other more primitive pieces of data.</skos:definition>
        <skos:example>A BMI Calculator</skos:example>
        <skos:example xml:lang="en">A FRAX Fracture Risk Score Calculator</skos:example>
        <skos:example xml:lang="en">A Pain assessment scale</skos:example>
        <skos:example xml:lang="en">The Cockcroft-Gault equation</skos:example>
        <skos:example xml:lang="en">The Scoring Model used to calculate the CHA2DS2-VASc score</skos:example>
        <skos:prefLabel xml:lang="en">Assessment, Predictive and Inferential Models</skos:prefLabel>
        <skos:scopeNote xml:lang="en">This is a broad category that includes e.g.: 
a) Mathematical equations and models used in a clinical or health-related calculation
b) Clinical or health-related scoring formulas for the purposes of predicting or classifying something, often by assigning points based on observations (e.g. stroke risk, asthma severity, mortality); includes information about the required inputs and the interpretation of the resulting score/s  
c) Associations, Correlations and Causal Relationships between phenomena, some of which may not be directly observable</skos:scopeNote>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://ontology.mayo.edu/ontology/KAO/InferentialAsset"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#example"/>
        <owl:annotatedTarget xml:lang="en">The Cockcroft-Gault equation</owl:annotatedTarget>
