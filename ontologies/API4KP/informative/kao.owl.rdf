            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>semantic information model</rdfs:label>
    </owl:Class>
    


    <!-- http://ckm.ontology.mayo.edu/ontology/KAO#SemanticServiceDescription -->

    <owl:Class rdf:about="http://ckm.ontology.mayo.edu/ontology/KAO#SemanticServiceDescription">
        <rdfs:subClassOf rdf:resource="http://ckm.ontology.mayo.edu/ontology/KAO#ServiceDescription"/>
        <rdfs:label>semantic service description</rdfs:label>
    </owl:Class>
    


    <!-- http://ckm.ontology.mayo.edu/ontology/KAO#ServiceDescription -->

    <owl:Class rdf:about="http://ckm.ontology.mayo.edu/ontology/KAO#ServiceDescription">
        <rdfs:subClassOf rdf:resource="http://ckm.ontology.mayo.edu/ontology/KAO#ProcessAsset"/>
        <rdfs:label>service description</rdfs:label>
    </owl:Class>
    


    <!-- http://ckm.ontology.mayo.edu/ontology/KAO#ServiceProfile -->

    <owl:Class rdf:about="http://ckm.ontology.mayo.edu/ontology/KAO#ServiceProfile">
        <rdfs:subClassOf rdf:resource="http://ckm.ontology.mayo.edu/ontology/KAO#ServiceDescription"/>
