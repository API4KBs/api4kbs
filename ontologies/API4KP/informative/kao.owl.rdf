    <!-- https://www.omg.org/spec/API4KP/api4kp/has-part -->

    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/API4KP/api4kp/has-part"/>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp/part-of -->

    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/API4KP/api4kp/part-of"/>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp/uses-concept -->

    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/API4KP/api4kp/uses-concept"/>
    


