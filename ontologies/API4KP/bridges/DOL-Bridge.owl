############################
#   Object Properties
############################

# Object Property: know:serialization-of (is concrete representation of)

SubObjectPropertyOf(know:serialization-of dol:isLinguisticallyRelatedTo)

# Object Property: know:serialized-into (concretely represented as)

SubObjectPropertyOf(know:serialized-into dol:isLinguisticallyRelatedTo)

# Object Property: know:serializes (serializes)

SubObjectPropertyOf(know:serializes dol:isLinguisticallyRelatedTo)
ObjectPropertyRange(know:serializes dol:Language)

# Object Property: know:specified-by (specified by)

ObjectPropertyDomain(know:specified-by dol:Language)

# Object Property: know:supports-serialization (supports serialization in)

SubObjectPropertyOf(know:supports-serialization dol:isLinguisticallyRelatedTo)
ObjectPropertyDomain(know:supports-serialization dol:Language)

# Object Property: know:uses-concrete-syntax (uses concrete syntax)

SubObjectPropertyOf(know:uses-concrete-syntax dol:isLinguisticallyRelatedTo)

# Object Property: dol:isSerializedIn (dol:isSerializedIn)

SubObjectPropertyOf(dol:isSerializedIn know:is-serialized-in)

# Object Property: dol:serializes (dol:serializes)

SubObjectPropertyOf(dol:serializes know:serializes)

# Object Property: dol:supportsSerialization (dol:supportsSerialization)

SubObjectPropertyOf(dol:supportsSerialization know:supports-serialization)


############################
#   Classes
############################

# Class: krr:Mapping (mapping)

EquivalentClasses(krr:Mapping dol:Mapping)

# Class: know:AbstractSyntax (Abstract Syntax)

SubClassOf(know:AbstractSyntax dol:LinguisticEntity)

# Class: know:ConstructedLanguage (Constructed Language)

EquivalentClasses(know:ConstructedLanguage dol:FormalLanguage)

# Class: know:ControlledNaturalLanguage (Controlled Natural Language)

EquivalentClasses(know:ControlledNaturalLanguage ObjectIntersectionOf(know:ConstructedLanguage ObjectSomeValuesFrom(dol:isSubLanguageOf know:NaturalLanguage)))

# Class: know:FormalLanguage (Formal Language)

EquivalentClasses(know:FormalLanguage ObjectSomeValuesFrom(dol:supportsLogic dol:Logic))

# Class: know:Language (language)

EquivalentClasses(know:Language dol:Language)

# Class: know:Model (model)

EquivalentClasses(know:Model <http://www.omg.org/spec/DOL/DOL-terms/MDEModel>)

# Class: know:Ontology (ontology)

EquivalentClasses(know:Ontology <http://www.omg.org/spec/DOL/DOL-terms/Ontology>)

# Class: know:Serialization (Concrete Syntax)

SubClassOf(know:Serialization dol:LinguisticEntity)
SubClassOf(know:Serialization ObjectSomeValuesFrom(know:serializes dol:Language))
SubClassOf(know:Serialization ObjectSomeValuesFrom(dol:isMappableTo know:AbstractSyntax))

# Class: know:Software (know:Software)

EquivalentClasses(know:Software ObjectIntersectionOf(know:ConcreteExpression ObjectSomeValuesFrom(dol:usesLanguage know:MachineExecutableLanguage)))

# Class: know:Specification (specification)

EquivalentClasses(know:Specification <http://www.omg.org/spec/DOL/DOL-terms/Specification>)

# Class: dol:AbstractSyntax (dol:AbstractSyntax)

SubClassOf(dol:AbstractSyntax know:AbstractSyntax)

# Class: dol:Expression (dol:Expression)

SubClassOf(dol:Expression krr:SerializedMachinableKnowledgeExpression)

# Class: dol:NativeOMS (dol:NativeOMS)

SubClassOf(dol:NativeOMS krr:NativeMachinableKnowledgeResource)

# Class: dol:OMS (dol:OMS)

SubClassOf(dol:OMS know:AbstractExpression)

# Class: dol:OMSLanguage (dol:OMSLanguage)

SubClassOf(dol:OMSLanguage krr:KnowledgeRepresentationAndReasoningLanguage)
SubClassOf(dol:OMSLanguage ObjectSomeValuesFrom(know:specified-by dol:AbstractSyntax))

# Class: dol:OMSSerialization (dol:OMSSerialization)

SubClassOf(dol:OMSSerialization krr:KnowledgeSerialization)

# Class: dol:Query (dol:Query)

EquivalentClasses(dol:Query ObjectIntersectionOf(ObjectSomeValuesFrom(dol:usesLanguage krr:QueryLanguage) ObjectAllValuesFrom(dol:usesLanguage krr:QueryLanguage)))

# Class: dol:Serialization (dol:Serialization)

SubClassOf(dol:Serialization know:Serialization)


SubObjectPropertyOf(ObjectPropertyChain(krr:hasEnvironmentMapping dol:mapsFrom) krr:hasEnvironmentMember)
SubObjectPropertyOf(ObjectPropertyChain(krr:hasEnvironmentMapping dol:mapsTo) krr:hasEnvironmentMember)
)