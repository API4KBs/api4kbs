<!DOCTYPE rdf:RDF [
	<!ENTITY AnnotationVocabulary "https://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY AnnotationVocabulary1 "http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY api4kp "https://www.omg.org/spec/API4KP/api4kp/">
	<!ENTITY api4kp-rel "https://www.omg.org/spec/API4KP/api4kp-rel/">
	<!ENTITY api4kp-series "https://www.omg.org/spec/API4KP/api4kp-series/">
	<!ENTITY dct "http://purl.org/dc/terms/">
	<!ENTITY dol "http://www.omg.org/spec/DOL/DOL-terms/">
	<!ENTITY lcc-lr "https://www.omg.org/spec/LCC/Languages/LanguageRepresentation/">
	<!ENTITY olex "http://www.w3.org/ns/lemon/ontolex#">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY sm "http://www.omg.org/techprocess/ab/SpecificationMetadata/">
	<!ENTITY time "http://www.w3.org/2006/time#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xml:base="https://www.omg.org/spec/API4KP/api4kp-series/"
	xmlns="https://www.omg.org/spec/API4KP/api4kp-series/"
	xmlns:AnnotationVocabulary="https://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
	xmlns:AnnotationVocabulary1="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
	xmlns:api4kp="https://www.omg.org/spec/API4KP/api4kp/"
	xmlns:api4kp-rel="https://www.omg.org/spec/API4KP/api4kp-rel/"
	xmlns:api4kp-series="https://www.omg.org/spec/API4KP/api4kp-series/"
	xmlns:dct="http://purl.org/dc/terms/"
	xmlns:dol="http://www.omg.org/spec/DOL/DOL-terms/"
	xmlns:lcc-lr="https://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"
	xmlns:olex="http://www.w3.org/ns/lemon/ontolex#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
	xmlns:time="http://www.w3.org/2006/time#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="https://www.omg.org/spec/API4KP/api4kp-series/">
		<rdfs:label>API4KP Ontology of Series</rdfs:label>
		<dct:abstract>TODO</dct:abstract>
		<dct:creator>Davide Sottara (sottara.davide at mayo.edu)</dct:creator>
		<dct:license rdf:datatype="&xsd;anyURI">http://opensource.org/licenses/MIT</dct:license>
		<sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.w3.org/standards/techs/owl#w3c_all</sm:contentLanguage>
		<sm:copyright>Copyright (c) 2014-2020 88 Solutions</sm:copyright>
		<sm:copyright>Copyright (c) 2014-2020 Adaptive</sm:copyright>
		<sm:copyright>Copyright (c) 2014-2020 Arizona State University</sm:copyright>
		<sm:copyright>Copyright (c) 2014-2020 Athan Services</sm:copyright>
		<sm:copyright>Copyright (c) 2014-2020 Fraunhofer FOKUS</sm:copyright>
		<sm:copyright>Copyright (c) 2014-2020 Freie Universitat Berlin</sm:copyright>
		<sm:copyright>Copyright (c) 2014-2020 Mayo Clinic</sm:copyright>
		<sm:copyright>Copyright (c) 2014-2020 Model Driven Solutions</sm:copyright>
		<sm:copyright>Copyright (c) 2014-2020 Thematix Partners LLC</sm:copyright>
		<sm:copyright>Copyright (c) 2015-2020 Object Management Group, Inc.</sm:copyright>
		<sm:copyright>Copyright (c) 2019-2020 Micro Focus</sm:copyright>
		<sm:dependsOn rdf:datatype="&xsd;anyURI">https://www.omg.org/spec/API4KP/api4kp/</sm:dependsOn>
		<sm:fileAbbreviation>api4kp-series</sm:fileAbbreviation>
		<sm:filename>api4kp-series.rdf</sm:filename>
		<owl:imports rdf:resource="http://www.omg.org/techprocess/ab/SpecificationMetadata/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
		<owl:versionIRI rdf:resource="https://www.omg.org/spec/API4KP/SNAPSHOT/api4kp-series/"/>
	</owl:Ontology>
	
	<owl:ObjectProperty rdf:about="&dol;hasPart">
	</owl:ObjectProperty>
	
	<owl:AnnotationProperty rdf:about="&skos;altLabel">
	</owl:AnnotationProperty>
	
	<owl:AnnotationProperty rdf:about="&skos;definition">
	</owl:AnnotationProperty>
	
	<owl:AnnotationProperty rdf:about="&skos;note">
	</owl:AnnotationProperty>
	
	<owl:AnnotationProperty rdf:about="&skos;scopeNote">
	</owl:AnnotationProperty>
	
	<owl:Class rdf:about="&time;Instant">
	</owl:Class>
	
	<owl:ObjectProperty rdf:about="&time;hasTime">
	</owl:ObjectProperty>
	
	<owl:Class rdf:about="&api4kp-series;ImmutableEntity">
		<owl:disjointWith rdf:resource="&api4kp-series;MutableEntity"/>
		<skos:definition>entity whose state does not, or cannot, change over time without preserving the identity of the entity</skos:definition>
		<skos:prefLabel>immutable</skos:prefLabel>
	</owl:Class>
	
	<owl:Class rdf:about="&api4kp-series;MutableEntity">
		<skos:definition>entity whose state, determined by the configuration of its qualia, changes over time while maintaining a principle of identity</skos:definition>
		<skos:prefLabel>mutable</skos:prefLabel>
	</owl:Class>
	
	<owl:Class rdf:about="&api4kp-series;Series">
		<rdfs:subClassOf rdf:resource="&api4kp-series;MutableEntity"/>
		<rdfs:subClassOf rdf:resource="&lcc-lr;Arrangement"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&api4kp-series;seriesOf"/>
				<owl:someValuesFrom rdf:resource="&api4kp-series;MutableEntity"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&lcc-lr;hasMember"/>
				<owl:someValuesFrom rdf:resource="&api4kp-series;version"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:comment>the set is implicitly equipped with a total order determined by the points in time at which the snapshots were taken</rdfs:comment>
		<skos:definition>the set of all the versions of the same entity</skos:definition>
		<skos:prefLabel>series</skos:prefLabel>
	</owl:Class>
	
	<owl:Class rdf:about="&api4kp-series;Snapshot">
		<rdfs:subClassOf rdf:resource="&api4kp-series;ImmutableEntity"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&time;hasTime"/>
				<owl:someValuesFrom rdf:resource="&time;Instant"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:seeAlso>https://en.wikipedia.org/wiki/Snapshot_(computer_storage)</rdfs:seeAlso>
		<skos:definition>immutable entity that reflects and maintains the state of a mutable entity at a particular point in time</skos:definition>
		<skos:prefLabel>snapshot</skos:prefLabel>
	</owl:Class>
	
	<owl:Class rdf:about="&api4kp-series;VersionIdentifier">
		<rdfs:subClassOf rdf:resource="&api4kp-series;ImmutableEntity"/>
		<rdfs:subClassOf rdf:resource="&lcc-lr;Identifier"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&dol;hasPart"/>
				<owl:someValuesFrom>
					<owl:Restriction>
						<owl:onProperty rdf:resource="&lcc-lr;identifies"/>
						<owl:someValuesFrom rdf:resource="&api4kp-series;MutableEntity"/>
					</owl:Restriction>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&lcc-lr;identifies"/>
				<owl:someValuesFrom rdf:resource="&api4kp-series;version"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<skos:altLabel>version-aware identifier</skos:altLabel>
		<skos:definition>identifier of a particular version of an entity, and has a part that identifies the entity (series) itself</skos:definition>
		<skos:example>the IRI / versionIRI pair used to identify versions of an OWL ontology</skos:example>
		<skos:prefLabel>version identifier</skos:prefLabel>
	</owl:Class>
	
	<owl:ObjectProperty rdf:about="&api4kp-series;hasSnapshot">
		<rdfs:range rdf:resource="&api4kp-series;Snapshot"/>
		<owl:inverseOf rdf:resource="&api4kp-series;isSnapshotOf"/>
		<skos:definition>had a state at some point in time that is reflected by the target immutable entity</skos:definition>
		<skos:prefLabel>has snapshot</skos:prefLabel>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&api4kp-series;hasVersion">
		<rdfs:subPropertyOf rdf:resource="&api4kp-series;hasSnapshot"/>
		<rdfs:range rdf:resource="&api4kp-series;version"/>
		<skos:definition>has a snapshot that is explicitly identified</skos:definition>
		<skos:prefLabel>has version</skos:prefLabel>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&api4kp-series;isSnapshotOf">
		<rdfs:domain rdf:resource="&api4kp-series;Snapshot"/>
		<rdfs:range rdf:resource="&api4kp-series;MutableEntity"/>
		<skos:definition>reflects the state of, at a particular point in time</skos:definition>
		<skos:prefLabel>is snapshot of</skos:prefLabel>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&api4kp-series;seriesOf">
		<rdfs:domain rdf:resource="&api4kp-series;Series"/>
		<rdfs:range rdf:resource="&api4kp-series;MutableEntity"/>
		<skos:definition>is the set of all versions of the same mutable entity</skos:definition>
		<skos:prefLabel>series of</skos:prefLabel>
	</owl:ObjectProperty>
	
	<owl:Class rdf:about="&api4kp-series;version">
		<rdfs:subClassOf rdf:resource="&api4kp-series;Snapshot"/>
		<owl:equivalentClass>
			<owl:Class>
				<owl:intersectionOf rdf:parseType="Collection">
					<rdf:Description rdf:about="&api4kp-series;Snapshot">
					</rdf:Description>
					<owl:Restriction>
						<owl:onProperty rdf:resource="&lcc-lr;isIdentifiedBy"/>
						<owl:someValuesFrom rdf:resource="&lcc-lr;Identifier"/>
					</owl:Restriction>
				</owl:intersectionOf>
			</owl:Class>
		</owl:equivalentClass>
		<skos:definition>snapshot that is assigned an identifier in some social context</skos:definition>
		<skos:prefLabel>version</skos:prefLabel>
	</owl:Class>

</rdf:RDF>