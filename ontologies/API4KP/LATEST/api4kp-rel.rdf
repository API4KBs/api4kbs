		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&api4kp;depends-on"/>
				<owl:someValuesFrom rdf:resource="&api4kp;KnowledgeRepresentation"/>
			</owl:Restriction>
		</rdfs:subClassOf>
	</owl:Class>
	
	<owl:Class rdf:about="&api4kp;ConcreteExpression">
	</owl:Class>
	
