    <!-- https://www.omg.org/spec/API4KP/api4kp-ops/hasOutcome -->

    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/API4KP/api4kp-ops/hasOutcome">
        <rdfs:domain rdf:resource="https://www.omg.org/spec/API4KP/api4kp/Activity"/>
        <rdfs:range rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/Outcome"/>
        <rdfs:label>has outcome</rdfs:label>
        <skos:definition>results in an Outcome at completion</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp-ops/hasTrace -->

    <owl:ObjectProperty rdf:about="https://www.omg.org/spec/API4KP/api4kp-ops/hasTrace">
