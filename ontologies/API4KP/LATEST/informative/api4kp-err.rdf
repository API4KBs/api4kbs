        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/KnowledgeResourceOutcome"/>
        <rdfs:label>compliance</rdfs:label>
        <skos:definition>outcome related to the ability to reproduce an expected behavior</skos:definition>
    </owl:Class>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp-ops/Conformance -->

    <owl:Class rdf:about="https://www.omg.org/spec/API4KP/api4kp-ops/Conformance">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/KnowledgeResourceOutcome"/>
        <rdfs:label>conformance</rdfs:label>
    </owl:Class>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp-ops/Consistency -->

    <owl:Class rdf:about="https://www.omg.org/spec/API4KP/api4kp-ops/Consistency">
