        <rdfs:label>consistency</rdfs:label>
    </owl:Class>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp-ops/Currency -->

    <owl:Class rdf:about="https://www.omg.org/spec/API4KP/api4kp-ops/Currency">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/Acceptance"/>
        <rdfs:label>currency</rdfs:label>
    </owl:Class>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp-ops/ExecutionTrace -->

    <owl:Class rdf:about="https://www.omg.org/spec/API4KP/api4kp-ops/ExecutionTrace">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp/KnowledgeExpression"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/playsRole"/>
                <owl:someValuesFrom rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/Explanation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>execution traces can be used as the explanation of an operation&apos;s result (explain what) and/or outcome (explain why)</rdfs:comment>
        <rdfs:label>execution trace</rdfs:label>
        <skos:definition>expression that describes the events that occurred during, and because of, the execution of a (knowledge processing) operation</skos:definition>
        <skos:editorialNote>TODO: promote to api4kp-ops</skos:editorialNote>
    </owl:Class>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp-ops/Existence -->

    <owl:Class rdf:about="https://www.omg.org/spec/API4KP/api4kp-ops/Existence">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/Resolvability"/>
        <rdfs:label>existence</rdfs:label>
    </owl:Class>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp-ops/FailureOutcome -->

    <owl:Class rdf:about="https://www.omg.org/spec/API4KP/api4kp-ops/FailureOutcome">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/ActivityOutcome"/>
        <rdfs:label>failure outcome</rdfs:label>
        <skos:altLabel>failure</skos:altLabel>
        <skos:definition>outcome that violates at least one of the goals associated to the activity that resulted in the outcome</skos:definition>
    </owl:Class>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp-ops/Fitness -->

    <owl:Class rdf:about="https://www.omg.org/spec/API4KP/api4kp-ops/Fitness">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/Acceptance"/>
        <rdfs:label>fitness</rdfs:label>
    </owl:Class>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp-ops/Grammaticality -->

    <owl:Class rdf:about="https://www.omg.org/spec/API4KP/api4kp-ops/Grammaticality">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/KnowledgeResourceOutcome"/>
        <rdfs:label>grammaticality</rdfs:label>
    </owl:Class>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp-ops/Identifiability -->

    <owl:Class rdf:about="https://www.omg.org/spec/API4KP/api4kp-ops/Identifiability">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/Resolvability"/>
        <rdfs:label>identifiability</rdfs:label>
    </owl:Class>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp-ops/KnowledgeResourceOutcome -->

    <owl:Class rdf:about="https://www.omg.org/spec/API4KP/api4kp-ops/KnowledgeResourceOutcome">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/Outcome"/>
        <rdfs:label>knowledge resource outcome</rdfs:label>
        <skos:definition>oucome state defined in terms of the state of syntactic (linguistic), semantic (domain/model) and/or pragmatic (formal) properties of a knowledge resource</skos:definition>
    </owl:Class>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp-ops/Locatability -->

    <owl:Class rdf:about="https://www.omg.org/spec/API4KP/api4kp-ops/Locatability">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/Resolvability"/>
        <rdfs:label>locatability</rdfs:label>
        <skos:definition>state related to the ability to acquire a given knowledge resource (version), pointer to, or copy thereof.</skos:definition>
        <skos:scopeNote>may be impacted by the derefencing process, but also to the relocation, or even non-existence of the knowledge base</skos:scopeNote>
    </owl:Class>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp-ops/Performance -->

    <owl:Class rdf:about="https://www.omg.org/spec/API4KP/api4kp-ops/Performance">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/Complexity"/>
        <rdfs:label>performance</rdfs:label>
    </owl:Class>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp-ops/ProfileConformance -->

    <owl:Class rdf:about="https://www.omg.org/spec/API4KP/api4kp-ops/ProfileConformance">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/Grammaticality"/>
        <rdfs:label>profile conformance</rdfs:label>
    </owl:Class>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp-ops/Resolvability -->

    <owl:Class rdf:about="https://www.omg.org/spec/API4KP/api4kp-ops/Resolvability">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/KnowledgeResourceOutcome"/>
        <rdfs:label>resolvability</rdfs:label>
    </owl:Class>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp-ops/Satisfiability -->

    <owl:Class rdf:about="https://www.omg.org/spec/API4KP/api4kp-ops/Satisfiability">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/Conformance"/>
        <rdfs:label>satisfiability</rdfs:label>
        <rdfs:seeAlso>http://www.cs.swansea.ac.uk/~csmarkus/ProcessesAndData/sites/default/files/uploads/papers/catesbeiana09.pdf</rdfs:seeAlso>
        <skos:definition>outcome related to the possibility of selecting and/or deriving the information necessary to bind one or more variables</skos:definition>
    </owl:Class>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp-ops/Soundness -->

    <owl:Class rdf:about="https://www.omg.org/spec/API4KP/api4kp-ops/Soundness">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/Compliance"/>
        <rdfs:label>soundness</rdfs:label>
    </owl:Class>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp-ops/State -->

    <owl:Class rdf:about="https://www.omg.org/spec/API4KP/api4kp-ops/State">
        <rdfs:label>state</rdfs:label>
        <skos:definition>specific configuration of an Entity&apos;s characteristics that holds over a period of time</skos:definition>
        <skos:scopeNote>this is a placeholder class for integration with ontologies that provider more specific definitions for the notion of state</skos:scopeNote>
    </owl:Class>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp-ops/SuccessfulOutcome -->

    <owl:Class rdf:about="https://www.omg.org/spec/API4KP/api4kp-ops/SuccessfulOutcome">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/ActivityOutcome"/>
        <rdfs:label>success outcome</rdfs:label>
        <skos:altLabel>success</skos:altLabel>
        <skos:definition>outcome that satisfies the goals associated to the activity that resulted in the outcome</skos:definition>
    </owl:Class>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp-ops/TheoreticalComplexity -->

    <owl:Class rdf:about="https://www.omg.org/spec/API4KP/api4kp-ops/TheoreticalComplexity">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/Complexity"/>
        <rdfs:label>theoretical complexity</rdfs:label>
    </owl:Class>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp-ops/Utility -->

    <owl:Class rdf:about="https://www.omg.org/spec/API4KP/api4kp-ops/Utility">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/Acceptance"/>
        <rdfs:label>utility</rdfs:label>
    </owl:Class>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp-ops/WellFormedness -->

    <owl:Class rdf:about="https://www.omg.org/spec/API4KP/api4kp-ops/WellFormedness">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/Grammaticality"/>
        <rdfs:label>well formedness</rdfs:label>
        <skos:definition>state defined by linguistic rules, including grammatical, syntactic and/or lexical constraints</skos:definition>
        <skos:scopeNote>this definition combines e.g. the XML-based notion of &apos;well-formedness&apos; vs &apos;validity&apos;, where the former is merely based on the concrete syntax/orthography rules, while the latter is based on the notion of abstract syntax/grammatical rules</skos:scopeNote>
    </owl:Class>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp-ops/Outcome -->

    <owl:Class rdf:about="https://www.omg.org/spec/API4KP/api4kp-ops/Outcome">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/State"/>
        <rdfs:label>outcome</rdfs:label>
        <skos:definition>state of a system that is the consequence of the execution of an activity, at the point in time the activity has been completed</skos:definition>
        <skos:scopeNote>the state is associated to, but not the same as, the point-in-time event of reaching that state, or the period of time over which the state holds</skos:scopeNote>
    </owl:Class>
    


    <!-- https://www.omg.org/spec/API4KP/api4kp/Activity -->

    <rdf:Description rdf:about="https://www.omg.org/spec/API4KP/api4kp/Activity">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/hasOutcome"/>
                <owl:someValuesFrom rdf:resource="https://www.omg.org/spec/API4KP/api4kp-ops/Outcome"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </rdf:Description>
</rdf:RDF>



