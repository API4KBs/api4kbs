                            <owl:Restriction>
                                <owl:onProperty rdf:resource="&ops;filled-by"/>
                                <owl:someValuesFrom rdf:resource="&lcc-lr;Language"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ops;has-role"/>
