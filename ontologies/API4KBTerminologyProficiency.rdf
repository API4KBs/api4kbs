<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY dct "http://purl.org/dc/terms/" >
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#" >
    <!ENTITY sm "http://www.omg.org/techprocess/ab/SpecificationMetadata/" >
    <!ENTITY ontoiop-trm "http://www.omg.org/spec/OntoIOp/OntoIOpTerminology/" >
    <!ENTITY apiforkb-trm "http://www.omg.org/spec/API4KB/API4KBTerminology/" >
    <!ENTITY apiforkb-op "http://www.omg.org/spec/API4KB/API4KBTerminology/API4KBOperation/" >
    <!ENTITY apiforkb-ev "http://www.omg.org/spec/API4KB/API4KBTerminology/API4KBEvent/" >
    <!ENTITY apiforkb-pro "http://www.omg.org/spec/API4KB/API4KBTerminology/API4KBProficiency/" >
]>

<rdf:RDF xml:base="&apiforkb-pro;"
  xmlns:rdf="&rdf;"
  xmlns:rdfs="&rdfs;"
  xmlns:owl="&owl;"
  xmlns:xsd="&xsd;"
  xmlns:dct="&dct;"
  xmlns:skos="&skos;"
  xmlns:sm="&sm;"
  xmlns:ontoiop-trm="&ontoiop-trm;"
  xmlns:apiforkb-trm="&apiforkb-trm;"
  xmlns:apiforkb-op="&apiforkb-op;"
  xmlns:apiforkb-ev="&apiforkb-ev;"
  xmlns:apiforkb-pro="&apiforkb-pro;">
  

  <owl:Ontology rdf:about="&apiforkb-pro;">
        <rdfs:label>API4KB Proficiency Terminology Ontology</rdfs:label>


    <!-- Curation and Rights Metadata for the API4KB Proficiency Terminology Ontology -->

        <sm:copyright rdf:datatype="&xsd;string">Copyright (c) 2014 Athan Services
Copyright (c) 2014 Thematix Partners LLC
Copyright (c) 2014 Object Management Group, Inc.</sm:copyright>
        <dct:license rdf:datatype="&xsd;anyURI">http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense</dct:license>


    <!-- Ontology/File-Level Metadata for the API4KB Proficiency Terminology Ontology -->

        <sm:filename rdf:datatype="&xsd;string">API4KBTerminologyProficiency.rdf</sm:filename>
        <sm:fileAbbreviation rdf:datatype="&xsd;string">api4kb-op</sm:fileAbbreviation>
        <owl:versionIRI rdf:resource="http://www.omg.org/spec/API4KB/20150201/API4KBTerminology/API4KBProficiency/"/>
        <sm:fileAbstract rdf:datatype="&xsd;string">This ontology covers terminology used for describing the proficiencies defined by the API4KB specification.</sm:fileAbstract>

        <sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.w3.org/standards/techs/owl#w3c_all</sm:contentLanguage>
        <sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/ODM/</sm:contentLanguage>

        <owl:imports rdf:resource="http://www.omg.org/techprocess/ab/SpecificationMetadata/"/>
        <owl:imports rdf:resource="http://www.omg.org/spec/OntoIOp/OntoIOpTerminology/"/>
      
    </owl:Ontology>
 
  
  <!-- Proficiency -->
  
  <owl:Class rdf:about="&apiforkb-pro;Proficiency">
    <rdfs:label>proficiency</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;isActualizedBy"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-ev;PublicOperationEvent"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <skos:definition rdf:datatype="&xsd;string">functionality that is actualized by the events of one or more public operations.</skos:definition>
    <skos:scopeNote rdf:datatype="&xsd;string">Note: In our architecture, roficiencies are provided by a collection of components (which may contain just one component). Different collections of components may provide the same proficiency. The standard API4KB proficiencies will be enumerated in the specification.</skos:scopeNote>
  </owl:Class>  

  <owl:Class rdf:about="&apiforkb-pro;SideEffectFreeProficiency">
    <rdfs:label>side-effect-free proficiency</rdfs:label>
    <rdfs:subClassOf rdf:resource="&apiforkb-pro;Proficiency"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;isActualizedBy"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-ev;SideEffectFreeEvent"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
  <owl:Class rdf:about="&apiforkb-pro;SideEffectfulProficiency">
    <rdfs:label>side-effectful proficiency</rdfs:label>
    <rdfs:subClassOf rdf:resource="&apiforkb-pro;Proficiency"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;isActualizedBy"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-ev;SideEffectfulEvent"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="&apiforkb-pro;UnaryProficiency">
    <rdfs:label>unary proficiency</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">unary proficiency</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-pro;Proficiency"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;isActualizedBy"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-ev;UnaryEvent"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class> 


  <!-- Script Output -->
  
  
  <owl:Class rdf:about="&apiforkb-pro;AddingProficiency-KnowledgeResourceToImmutableKB">
    <rdfs:label>proficiency adding knowledge resources to immutable KBs</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">proficiency of adding knowlede
      resources to immutable knowledge bases</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-pro;Proficiency"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;isActualizedBy"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-ev;AddingEvent-KnowledgeResourceToImmutableKB"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>  
  
  <owl:Class rdf:about="&apiforkb-pro;AddingProficiency-QueryItemToImmutableKB">
    <rdfs:label>proficiency of adding knowledge resources that are query items to immutable KBs</rdfs:label>
    <rdfs:subClassOf rdf:resource="&apiforkb-pro;AddingProficiency-KnowledgeResourceToImmutableKB"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;isActualizedBy"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-ev;AddingEvent-QueryItemToImmutableKB"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
  
  <owl:Class rdf:about="&apiforkb-pro;AddingProficiency-FocusedItemToImmutableKB">
    <rdfs:label>proficiency of adding knowledge resources that are focused items to immutable KBs</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">proficiency of adding knowledge resources that are focused items to immutable KBs</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-pro;AddingProficiency-KnowledgeResourceToImmutableKB"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;isActualizedBy"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-ev;AddingEvent-FocusedItemToImmutableKB"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
  <owl:Class rdf:about="&apiforkb-pro;AddingProficiency-KnowledgeResourceToMutableKB">
    <rdfs:label>proficiency of adding knowledge resources to mutable KBs</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">proficiency of adding knowlede
      resources to mutable KBs</skos:definition>
    <dct:abstract/>
    <rdfs:subClassOf rdf:resource="&apiforkb-pro;Proficiency"/>
    <rdfs:subClassOf rdf:resource="&apiforkb-pro;SideEffectfulProficiency"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;isActualizedBy"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-ev;AddingEvent-KnowledgeResourceToMutableKB"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
</rdf:RDF>
