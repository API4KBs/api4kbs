    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasEnglishName -->

    <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasEnglishName">
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasName"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Section 3.4, ISO 639-3</dct:source>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>has English name</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">associates a name in English with an individual concept</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasFrenchName -->

    <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasFrenchName">
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasName"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Section 3.4, ISO 639-3</dct:source>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>has French name</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">associates a name in French with an individual concept</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasGermanName -->

    <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasGermanName">
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasName"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>has German name</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">associates a name in German with an individual concept</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasIndigenousName -->

    <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasIndigenousName">
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasName"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Section 3.4, ISO 639-3</dct:source>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>has indigenous name</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">associates a local regional or cultural name with an individual concept</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasName -->

    <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Section 3.4, ISO 639-3</dct:source>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>has name</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">associates a name, reference name, or appellation with an individual concept</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasTag -->

    <owl:DatatypeProperty rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasTag">
        <rdfs:domain rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Identifier"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>has tag</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a unique combination of alphanumeric characters corresponding to the identifier</skos:definition>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Text-valued tags are included here as they may be useful for automated transformation or encoding systems, such as those used to produce IETF compliant language tags in XML.</skos:note>
    </owl:DatatypeProperty>
    


