    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/denotes -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/denotes">
        <owl:inverseOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasDenotation"/>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>denotes</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">serves as a linguistic expression of the notion of, means</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/has -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/has">
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>has</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">indicates that someone (or something) possesses something, such as a characteristic, attribute, feature, or capability</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasDenotation -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasDenotation">
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/has"/>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>has denotation</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has meaning, expresses</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasEarlierForm -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasEarlierForm">
        <rdfs:subPropertyOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/has"/>
        <rdfs:domain rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Language"/>
        <rdfs:range rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Language"/>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>has earlier form</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">relates a language to an earlier form (historically significant) of that same language</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasMember -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasMember">
        <owl:inverseOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/isMemberOf"/>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>has member</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">relates something, typically a collection, group or organization, to some discrete thing identified as a member of it</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/identifies -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/identifies">
        <owl:inverseOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/isIdentifiedBy"/>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>identifies</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">recognizes or establishes within some context</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/isIdentifiedBy -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/isIdentifiedBy">
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>is identified by</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has an indicator or label, that is unique within some context</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/isMemberOf -->

    <owl:ObjectProperty rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/isMemberOf">
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>is member of</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">belongs, either individually or collectively, to a group</skos:definition>
    </owl:ObjectProperty>
    


