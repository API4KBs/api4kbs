    <!-- http://www.omg.org/spec/API4KP/core#Grammar -->

    <owl:Class rdf:about="http://www.omg.org/spec/API4KP/core#Grammar"/>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Alpha2Code -->

    <owl:Class rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Alpha2Code">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/LanguageIdentifier"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Section 4.1, ISO 639-1</dct:source>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>alpha-2 code</rdfs:label>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/UTF-8</rdfs:seeAlso>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a language identifier for a human language as defined by ISO 639-1, that is, a two-letter combination of characters used to represent a language or languages</skos:definition>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema#string">In the ISO 639-1 language code, each language identifier is composed of two letters (lower case letters from the Latin alphabet, corresponding to characters 97 through 122 of the UTF-8 encoding), without diacritical marks or other encodings of any kind.</skos:note>
        <skos:scopeNote rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The identifiers are not abbreviations for the languages they identify.  They are derived in some way from the indiginous language name or from preferences of the relevant speech community.</skos:scopeNote>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Alpha3Code -->

    <owl:Class rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Alpha3Code">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/LanguageIdentifier"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Section 3.2, ISO 639-2</dct:source>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>alpha-3 code</rdfs:label>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/UTF-8</rdfs:seeAlso>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a language identifier for a human language as defined by ISO 639, that is, a three-letter combination of characters used to represent a language or languages</skos:definition>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema#string">In the ISO 639-2 language code, each language identifier is composed of three letters (lower case letters from the Latin alphabet, corresponding to characters 97 through 122 of the UTF-8 encoding), without diacritical marks or other encodings of any kind.</skos:note>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Two code sets are provided in ISO 639-2, one for bibliographic applications and one for terminology applications. Some of these are considered collective language codes, referring to a family of languages, and there are a few special purpose codes for use in various applications. ISO 639-3 provides a single, three character code for each individual language or macrolanguage. Additional codes for language families and groups are specified in other parts of the standard.</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/AncientLanguage -->

    <owl:Class rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/AncientLanguage">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/ExtinctLanguage"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Section 3.25 and 4.7, ISO 639-4</dct:source>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>ancient language</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">an extinct language that has an attested literature and is recognized as having special status in the academic community</skos:definition>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ancient languages may have either individual or collective language codes.  See section 4.1.5, ISO 639-2, for an explanation of the latter case.</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Arrangement -->

    <owl:Class rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Arrangement">
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>arrangement</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a structure or means of organizing information such as a schema, numbering system, organization scheme, measurement system, taxonomy, or language for organizing information</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/ArtificialLanguage -->

    <owl:Class rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/ArtificialLanguage">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Language"/>
        <owl:disjointWith rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/NaturalLanguage"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Section 3.28 and 4.8, ISO 639-4</dct:source>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>artificial language</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a language for human communication that has been artificially devised</skos:definition>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Artificial languages do not include reconstructed languages or computer programming languages.</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/BibliographicCode -->

    <owl:Class rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/BibliographicCode">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Alpha3Code"/>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/LanguageIdentifier"/>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>bibliographic code</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">an alpha-3 code that is a member of the set of bibliographic codes in ISO 639-2</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/CodeElement -->

    <owl:Class rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/CodeElement">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/denotes"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/isMemberOf"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/CodeSet"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISO/IEC 11179-3 Information technology - Metadata registries (MDR) - Part 3: Registry metamodel and basic attributes, Third edition, 2013-02-15</dct:source>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>code element</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a sequence of characters denoting something that it is associated with for some purpose, within a specified context, according to some rule set</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/CodeSet -->

    <owl:Class rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/CodeSet">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Arrangement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasMember"/>
                <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/CodeElement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISO/IEC 11179-3 Information technology - Metadata registries (MDR) - Part 3: Registry metamodel and basic attributes, Third edition, 2013-02-15</dct:source>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>code set</rdfs:label>
        <skos:altLabel rdf:datatype="http://www.w3.org/2001/XMLSchema#string">coding scheme</skos:altLabel>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a system of alpha-numeric symbols, or combinations of symbols, that stand for specified values in some context</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Collection -->

    <owl:Class rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Collection">
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>collection</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a grouping of some variable number of things (may be zero) that have some shared significance</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/CollectiveLanguageCode -->

    <owl:Class rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/CollectiveLanguageCode">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/LanguageIdentifier"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/denotes"/>
                <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/LanguageGroup"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/identifies"/>
                <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/LanguageGroup"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/SpecialPurposeCode"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sections 3.3 and 4.1.1, and 4.1.3, ISO 639-2; Section 4.6, ISO 639-4</dct:source>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>collective language code</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a language identifier or code used to represent a group of languages</skos:definition>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema#string">See section 4.1.1, ISO 639-2, for an explanation of the usage of such codes, and section 4.1.3 to gain understanding of the distinction between collective language and macrolanguage codes.</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Dialect -->

    <owl:Class rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Dialect">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/LanguageVariant"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sections 3.8, 4.2, and 4.5, ISO 639-4; Sections 4.1.3 and 4.1.4, ISO 639-2</dct:source>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>dialect</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a language variant that is specific to a geographical region or speech community</skos:definition>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dialects are typically represented via the same language code as the code for the primary language, with a few exceptions for well-known dialects.  See sections 4.1.3 and 4.1.4, ISO 639-2 for additional details on the treatment of dialects in the standard.</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/ExtinctLanguage -->

    <owl:Class rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/ExtinctLanguage">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Language"/>
        <owl:disjointWith rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/LivingLanguage"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Section 3.24 and 4.7, ISO 639-4</dct:source>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>extinct language</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">an individual language or variant that is no longer in use and has no current descendant</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/HistoricalLanguage -->

    <owl:Class rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/HistoricalLanguage">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Language"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Section 3.26 and 4.7, ISO 639-4</dct:source>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>historical language</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a well-known prior version of a living or extinct language</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/IdentificationScheme -->

    <owl:Class rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/IdentificationScheme">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Arrangement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasMember"/>
                <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Identifier"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISO/IEC 11179-3 Information technology - Metadata registries (MDR) - Part 3: Registry metamodel and basic attributes, Third edition, 2013-02-15</dct:source>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>identification scheme</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a system for allocating identifiers to objects</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Identifier -->

    <owl:Class rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Identifier">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/identifies"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/isMemberOf"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/IdentificationScheme"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasTag"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISO/IEC 11179-3 Information technology - Metadata registries (MDR) - Part 3: Registry metamodel and basic attributes, Third edition, 2013-02-15</dct:source>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>identifier</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a sequence of characters uniquely identifying something that it is associated with for some purpose and within a specified context</skos:definition>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Text-valued tags are included here as they may be useful for automated transformation or encoding systems, such as those used to produce IETF compliant language tags in XML.</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/IndividualLanguage -->

    <owl:Class rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/IndividualLanguage">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Language"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Section 3.7, ISO 639-4</dct:source>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>individual language</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a language that can be distinguished from another language by some set of rules</skos:definition>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Identifiers in Parts 1, 2, and 3 of ISO 639 are assumed to denote distinct individual languages, unless the language name explicitly refers to a language group.  See section 4.2 in ISO 639-4 for a detailed explanation of the definition of individual language in the context of the ISO 639 standard.</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/IndividualLanguageIdentifier -->

    <owl:Class rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/IndividualLanguageIdentifier">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/LanguageIdentifier"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/denotes"/>
                <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/IndividualLanguage"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/identifies"/>
                <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/IndividualLanguage"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/MacrolanguageIdentifier"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Section 3.7, ISO 639-3</dct:source>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>individual language identifier</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a language identifier whose scope is that of an individual (distinct) language</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Language -->

    <owl:Class rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Language">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasEnglishName"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasFrenchName"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasGermanName"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/hasIndigenousName"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Section 3.6, ISO 639-4</dct:source>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>language</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a systematic use of sounds, characters, symbols or signs to communicate meaning</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/LanguageFamily -->

    <owl:Class rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/LanguageFamily">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/LanguageGroup"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Section 3.11, ISO 639-4</dct:source>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>language family</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a group of individual languages related to each other through common ancestry</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/LanguageGroup -->

    <owl:Class rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/LanguageGroup">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Collection"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Section 3.10, ISO 639-4</dct:source>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>language group</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a collection of two or more individual languages, treated as a group for some purpose</skos:definition>
        <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema#string">See section 4.6 in ISO 639-4 for additional explanatory material for language groups.</skos:note>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/LanguageIdentifier -->

    <owl:Class rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/LanguageIdentifier">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/CodeElement"/>
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Identifier"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/denotes"/>
                <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Language"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/identifies"/>
                <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Language"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Section 3.5, ISO 639-4</dct:source>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>language identifier</rdfs:label>
        <skos:altLabel rdf:datatype="http://www.w3.org/2001/XMLSchema#string">language symbol</skos:altLabel>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a string of characters that uniquely identifies a linguistic entity</skos:definition>
        <skos:prefLabel rdf:datatype="http://www.w3.org/2001/XMLSchema#string">language identifier</skos:prefLabel>
        <skos:scopeNote rdf:datatype="http://www.w3.org/2001/XMLSchema#string">In the language codes of Parts 1, 2, 3, and 5 of ISO 639, each language identifier is composed of two or three letters.</skos:scopeNote>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/LanguageVariant -->

    <owl:Class rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/LanguageVariant">
        <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/IndividualLanguage"/>
        <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Section 3.14, ISO 639-4</dct:source>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
        <rdfs:label>language variant</rdfs:label>
        <skos:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a variation of an individual language that is sufficiently unique that it can be identified and named</skos:definition>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/LanguageVariation -->

    <owl:Class rdf:about="http://www.omg.org/spec/LCC/Languages/LanguageRepresentation/LanguageVariation">
