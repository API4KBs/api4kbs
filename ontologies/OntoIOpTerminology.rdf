     -->

    


    <!-- http://www.w3.org/2000/01/rdf-schema#subClassOf -->

    <owl:AnnotationProperty rdf:about="&rdfs;subClassOf"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/OMSUnionOf -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;OMSUnionOf">
        <rdfs:label>is union of OMSs</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string"> \termdefinition{union}{aggregation of several \termref{OMSs}
    to a new \termref{OMS}, without any renaming}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
        <rdfs:range rdf:resource="&ontoiop-trm;SetOfOMSs"/>
        <rdfs:domain rdf:resource="&ontoiop-trm;StructuredOMS"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/approximantOf -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;approximantOf">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label>is approximant of</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">\termdefinition{approximant}{approximation (in the sense of a logically implied theory, possibly after
suitable translation)
of an OMS in a smaller \termref{signature} or
\termref{OMS language}}
\termdefinition{maximum approximant}{
best possible (in the sense of a maximum set of logical consequences)
\termref{approximant} of an OMS in a smaller \termref{signature} or \termref{OMS language}}
\begin{note}
 Technically, a maximum approximant is a uniform interpolant, see \cite{DBLP:conf/ijcai/LutzW11}.
\end{note}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
        <rdfs:range rdf:resource="&ontoiop-trm;OMS"/>
        <rdfs:domain rdf:resource="&ontoiop-trm;OMS"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/combinationOf -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;combinationOf">
        <rdfs:label>is combination of</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string"> \termdefinition{combination}{aggregation of all the OMS in a \termref{OMS graph}, where \termref{non-logical symbol}s are shared according to the \termref{OMS mapping}s in the OMS graph}
\begin{example}
Consider an ontology involving a concept \texttt{Person},
and another one involving \texttt{Human being}, and an \termref{alignment}
that relates these to concepts. In the combination of the ontologies
along the alignment, there is only one concept, representing both
\texttt{Person} and \texttt{Human being}.
\end{example}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
        <rdfs:range rdf:resource="&ontoiop-trm;SetOfOMSs"/>
        <rdfs:domain rdf:resource="&ontoiop-trm;StructuredOMS"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/documentationOf -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;documentationOf">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>is documentation of</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">see Class &apos;OMS documentaton&apos;</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
        <rdfs:range rdf:resource="&ontoiop-trm;OMS"/>
        <rdfs:domain rdf:resource="&ontoiop-trm;OMSDocumentation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/extensionOf -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;extensionOf">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label>is extension of</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string"> \termdefinition{extension}{\termref{OMS} whose sets of \termref{non-logical symbols} and \termref{sentences} are supersets of those present in a given smaller \termref{OMS}}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
        <rdfs:range rdf:resource="&ontoiop-trm;OMS"/>
        <rdfs:domain rdf:resource="&ontoiop-trm;OMS"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/forType -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;forType">
        <rdfs:label>for type</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">abstract property for the relation between an &quot;implementation&quot; of a generic class and its type parameters</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/forType1 -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;forType1">
        <rdfs:label>for first type</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">the first type parameter in the type signature of the implementation of a generic class</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
        <rdfs:subPropertyOf rdf:resource="&ontoiop-trm;forType"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/formalizedBy -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;formalizedBy">
        <rdfs:label>is formalized by</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">institution that formalizes a logic</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
        <rdfs:range rdf:resource="&ontoiop-trm;Institution"/>
        <rdfs:domain rdf:resource="&ontoiop-trm;Logic"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/formalizes -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;formalizes">
        <rdfs:label>formalizes</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">see Class &apos;institution&apos;</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
        <owl:inverseOf rdf:resource="&ontoiop-trm;formalizedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/hasAspect -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;hasAspect">
        <rdfs:label>has language aspect</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">see Class &apos;language aspect&apos;</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
        <rdfs:range rdf:resource="&ontoiop-trm;LanguageAspect"/>
        <rdfs:domain rdf:resource="&ontoiop-trm;OMSLanguage"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/hasLogic -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;hasLogic">
        <rdfs:label>has logic</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">logic represented by a satisfaction relation of the language.</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
        <rdfs:range rdf:resource="&ontoiop-trm;Logic"/>
        <rdfs:domain rdf:resource="&ontoiop-trm;OMSLanguage"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/hasLogicalEnvironment -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;hasLogicalEnvironment">
        <rdfs:label>has logical environment</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">logical environment in which the subject is embedded or is associated with.</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/hasSetOfExpressions -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;hasSetOfExpressions">
        <rdfs:label>has set of expressions</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">set of expressions that is a component or association of the subject.</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/hasSetOfMappings -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;hasSetOfMappings">
        <rdfs:label>has set of mappings</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">set of mappings that is a component or association of the subject.</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/hasSubset -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;hasSubset">
        <rdfs:label>has subset</rdfs:label>
        <rdfs:domain rdf:resource="&ontoiop-trm;TypedSet"/>
        <rdfs:range rdf:resource="&ontoiop-trm;TypedSet"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/isDefinitionalExtensionOf -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;isDefinitionalExtensionOf">
        <rdfs:label>is definitional extension of</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">\termdefinition{definitional extension}{\termref{extension} whose newly introduced
   \termref{non-logical symbols} are interpreted in a unique way}
  \begin{note}
An \termref{extension} $O_2$ of an \termref{OMS} $O_1$ is a definitional extension, if each model of $O_1$ can be uniquely expanded to a model of $O_2$.
  \end{note}
  \begin{note}
    $O_2$ being a definitional extension of $O_1$ implies a bijective correspondence between the classes of models of $O_2$ and $O_1$.
  \end{note}
  \begin{note}
    Each definitional extension is also a monomorphic extension but not vice versa.
  \end{note}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
        <rdfs:subPropertyOf rdf:resource="&ontoiop-trm;extensionOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/isElementOf -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;isElementOf">
        <rdfs:label>is element Of</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">\termdefinition{element (of an OMS)}{any \termref{resource} in an \termref{OMS} (\eg a \termref{non-logical symbol}, a \termref{sentence}, a \termref{correspondence}, the \termref{OMS} itself, ...) or a named set of such \termref{resources}.}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
        <rdfs:range rdf:resource="&ontoiop-trm;OMS"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/isImpliedExtensionOf -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;isImpliedExtensionOf">
        <rdfs:label>is implied extension of</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string"> \termdefinition{implied extension}{\termref{model-theoretic conservative extension} that does not introduce new \termref{non-logical symbols}}
  \begin{note}
    A \termref{conservative extension} $O_2$ of an \termref{OMS}
    $O_1$ is an implied extension, if and only if the signature of
    $O_2$ is the signature of $O_1$. $O_2$ is an implied extension of
    $O_1$ if and only if the model class of $O_2$ is the model class
    of $O_1$.
  \end{note}
  \begin{note}
    Each implied extension is also a definitional extension but not vice versa.
  \end{note}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
        <rdfs:subPropertyOf rdf:resource="&ontoiop-trm;extensionOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/isLanguageAspectOf -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;isLanguageAspectOf">
        <rdfs:label>is language aspect of</rdfs:label>
        <owl:inverseOf rdf:resource="&ontoiop-trm;hasAspect"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/isModelOfSignature -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;isModelOfSignature">
        <rdfs:label>is model of signature</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">the signature (vocabulary) that is interpreted by the subect semantic interpretation.</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
        <rdfs:domain rdf:resource="&ontoiop-trm;Model"/>
        <rdfs:range rdf:resource="&ontoiop-trm;Vocabulary"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/logicallyInteroperableWith -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;logicallyInteroperableWith">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label>is logically interoperable with</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">TBD</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
        <rdfs:range rdf:resource="&ontoiop-trm;StructuredOMS"/>
        <rdfs:domain rdf:resource="&ontoiop-trm;StructuredOMS"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/mappingCodomain -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;mappingCodomain">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>has mapping codomain</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">the codomain of the morphism (as in Category Theory).</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
        <rdfs:domain rdf:resource="&ontoiop-trm;Mapping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/mappingDomain -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;mappingDomain">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>has mapping domain</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">&gt;the domain of the morphism (as in Category Theory).</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
        <rdfs:domain rdf:resource="&ontoiop-trm;Mapping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/profileOf -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;profileOf">
        <rdfs:label>is profile of</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">see Class &apos;profile&apos;</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
        <rdfs:range rdf:resource="&ontoiop-trm;OMSLanguage"/>
        <rdfs:domain rdf:resource="&ontoiop-trm;Profile"/>
        <rdfs:subPropertyOf rdf:resource="&ontoiop-trm;subLanguageOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/satisfactionRelation -->

    <owl:ObjectProperty rdf:about="&ontoiop-trm;satisfactionRelation">
        <rdfs:label>satisfaction relation</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">\termdefinition{satisfaction relation}{relation
      between models and sentences indicating which sentences hold true in the
      model}</skos:definition>
        <dct:abstract></dct:abstract>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
        <rdfs:domain rdf:resource="&ontoiop-trm;Model"/>
