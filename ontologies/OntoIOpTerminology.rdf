    <!-- http://www.omg.org/spec/OntoIOp/Terminology/AnnotationExpression -->

    <owl:Class rdf:about="&ontoiop-trm;AnnotationExpression">
        <rdfs:label>annotation expression</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ontoiop-trm;Expression"/>
        <skos:definition rdf:datatype="&xsd;string">\termdefinition{annotation}{additional information without a logical semantics that is attached to an \termref{element} of an \termref{OMS}}
  \begin{note}\label{note:annotation-rdf}
    Formally, an annotation is given as a $(\text{subject}, \text{predicate}, \text{object})$ triple as defined by \nisref{SOURCE: W3C/TR REC-rdf-concepts:2004, Section 6}. The subject of an annotation is an \termref{element} of an OMS. The predicate is an RDF property defined in an external OMS and describes in what way the annotation object is related to the annotation subject.
  \end{note}
  \begin{note}
    According to note~\ref{note:annotation-rdf} it is possible to interpret annotations under an RDF semantics. ``Without a logical semantics&apos;&apos; in this definition means that annotations to an OMS are not considered sentences of that OMS.
  \end{note}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/AnnotationExpressionLanguageAspect -->

    <owl:Class rdf:about="&ontoiop-trm;AnnotationExpressionLanguageAspect">
        <rdfs:label>annotation expression language Aspect</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ontoiop-trm;LanguageAspect"/>
        <owl:disjointWith rdf:resource="&ontoiop-trm;LogicalLanguageAspect"/>
        <owl:disjointWith rdf:resource="&ontoiop-trm;StructuringLanguageAspect"/>
        <skos:definition rdf:datatype="&xsd;string">\termdefinition{annotation language aspect}{the (unique) \termref{language aspect} of an \termref{OMS language} that enables the expression of comments and annotations}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/BasicOMS -->

    <owl:Class rdf:about="&ontoiop-trm;BasicOMS">
        <rdfs:label>basic OMS</rdfs:label>
        <owl:disjointWith rdf:resource="&ontoiop-trm;DistributedOMS"/>
        <owl:disjointWith rdf:resource="&ontoiop-trm;HeterogeneousOMS"/>
        <owl:disjointWith rdf:resource="&ontoiop-trm;StructuredOMS"/>
        <skos:definition rdf:datatype="&xsd;string">\termdefinition{basic OMS}{\termref{signature} equipped with a set of \termref{sentences} and \termref{annotations}, which may be used as a building block for a larger \termref{OMS}}
  \begin{note}
    The sentences must use only those non-logical symbols that are present in the signature.
  \end{note}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/DistributedOMS -->

    <owl:Class rdf:about="&ontoiop-trm;DistributedOMS">
        <rdfs:label>distributed OMS</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ontoiop-trm;OMS"/>
        <skos:definition rdf:datatype="&xsd;string"> \termdefinition{distributed OMS\synonym
      hyperontology}{collection of named \termref{OMSs,} possibly written in different \termref{OMS
      languages}, linked by named \termref{OMS mappings} and named \termref{OMS
      graphs}}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/Equivalence -->

    <owl:Class rdf:about="&ontoiop-trm;Equivalence">
        <rdfs:label>equivalence</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ontoiop-trm;LogicalOMSMapping"/>
        <skos:definition rdf:datatype="&xsd;string">        
      \termdefinition{equivalence}
      {\termref{logical OMS mapping} ensuring that two \termref{OMSs} share the
      same definable concepts}
      \begin{note}
      Two OMSs are equivalent if they have a common definitional extension.
      The OMSs may be written in different OMS languages.
      \end{note}
      </skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/Expression -->

    <owl:Class rdf:about="&ontoiop-trm;Expression">
        <rdfs:label>expression</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ontoiop-trm;usesLanguage"/>
                <owl:onClass rdf:resource="&ontoiop-trm;Language"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">see Class &apos;language&apos;</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/Generic -->

    <owl:Class rdf:about="&ontoiop-trm;Generic">
        <rdfs:label>class with generic types</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">class parameterized by one or more type parameters</skos:definition>
        <dct:abstract></dct:abstract>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/Generic1 -->

    <owl:Class rdf:about="&ontoiop-trm;Generic1">
        <rdfs:label>class with one generic type</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ontoiop-trm;Generic"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ontoiop-trm;forType1"/>
                <owl:onClass rdf:resource="&owl;Class"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">class parameterized by exactly one type parameters</skos:definition>
        <dct:abstract></dct:abstract>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/HeterogeneousOMS -->

    <owl:Class rdf:about="&ontoiop-trm;HeterogeneousOMS">
        <rdfs:label>heterogeneous OMS</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ontoiop-trm;OMS"/>
        <skos:definition rdf:datatype="&xsd;string">  \termdefinition{heterogeneous OMS}{\termref{OMS} whose parts are supported by different \termref{logics}}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/Institution -->

    <owl:Class rdf:about="&ontoiop-trm;Institution">
        <rdfs:label>institution</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ontoiop-trm;formalizes"/>
                <owl:someValuesFrom rdf:resource="&ontoiop-trm;Logic"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string"> \termdefinition{institution}{metaframework mathematically formalising
the notion of a \termref{logic}}
\begin{note}
 See clause~\ref{c:semantics} for a formal definition.
\end{note}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/Language -->

    <owl:Class rdf:about="&ontoiop-trm;Language">
        <rdfs:label>language</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">TBD</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/LanguageAspect -->

    <owl:Class rdf:about="&ontoiop-trm;LanguageAspect">
        <rdfs:label>language aspect</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">\termdefinition{language aspect}{set of language constructs of a given language, not necessarily forming a sublanguage}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/LanguageMapping -->

    <owl:Class rdf:about="&ontoiop-trm;LanguageMapping">
        <rdfs:label>language mapping</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ontoiop-trm;Mapping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ontoiop-trm;mappingCodomain"/>
                <owl:onClass rdf:resource="&ontoiop-trm;Language"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ontoiop-trm;mappingDomain"/>
                <owl:onClass rdf:resource="&ontoiop-trm;Language"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">maorphism of the Category &apos;language&apos;.</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/Logic -->

    <owl:Class rdf:about="&ontoiop-trm;Logic">
        <rdfs:label>logic</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ontoiop-trm;formalizedBy"/>
                <owl:someValuesFrom rdf:resource="&ontoiop-trm;Institution"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">  \termdefinition{logic}{specification of valid reasoning that comprises \termref{signatures}, \termref{sentences},
  \termref{models}, and a \termref{satisfaction relation} between \termref{models} and \termref{sentences}}
  \begin{note}
    Most OMS languages have an underlying logic.
  \end{note}
  \begin{example}
    $\mathcal{SROIQ}(D)$ is the logic underlying OWL 2 DL.
  \end{example}
  \begin{note}
   See annex~\ref{a:rdf-logic-vocab} for the organization of the
   relation between OMS languages and their logics and serializations.
  \end{note}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/LogicApproximation -->

    <owl:Class rdf:about="&ontoiop-trm;LogicApproximation">
        <rdfs:label>logic approximation</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ontoiop-trm;LogicMapping"/>
        <skos:definition rdf:datatype="&xsd;string">\termdefinition{logic approximation}{mapping of a source \termref{logic}
  onto a (usually less expressive) target \termref{logic} that tries
  to approximate the OMSs expressed in the source \termref{logic} with
  means of the expressivity of the target \termref{logic}}
\begin{note}
  A unique maximal approximation need not exist.
\end{note}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/LogicMapping -->

    <owl:Class rdf:about="&ontoiop-trm;LogicMapping">
        <rdfs:label>logic mapping</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ontoiop-trm;Mapping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ontoiop-trm;mappingCodomain"/>
                <owl:onClass rdf:resource="&ontoiop-trm;Logic"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ontoiop-trm;mappingDomain"/>
                <owl:onClass rdf:resource="&ontoiop-trm;Logic"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">morphism of the Category &apos;logic&apos;.</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/LogicReduction -->

    <owl:Class rdf:about="&ontoiop-trm;LogicReduction">
        <rdfs:label>logic reduction</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ontoiop-trm;LogicMapping"/>
        <skos:definition rdf:datatype="&xsd;string">  \termdefinition{logic reduction}{mapping of a source \termref{logic}
    onto a (usually less expressive) target \termref{logic} (mapping
    \termref{signatures}, \termref{sentences} and \termref{models})
    that simply forgets those parts of the logical structure not
    fitting the target \termref{logic}}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/LogicTranslation -->

    <owl:Class rdf:about="&ontoiop-trm;LogicTranslation">
        <rdfs:label>logic translation</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ontoiop-trm;LogicMapping"/>
        <skos:definition rdf:datatype="&xsd;string">  \termdefinition{logic translation}{mapping of a source
    \termref{logic} into a target \termref{logic} (mapping
    \termref{signatures}, \termref{sentences} and \termref{models})
    that keeps or encodes the logical content of \termref{OMSs}}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/LogicalEnvironment -->

    <owl:Class rdf:about="&ontoiop-trm;LogicalEnvironment">
        <rdfs:label>logical environment</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ontoiop-trm;SetOfMappings"/>
        <skos:definition rdf:datatype="&xsd;string">diafram in the Category &apos;institution&apos;.</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/LogicalLanguageAspect -->

    <owl:Class rdf:about="&ontoiop-trm;LogicalLanguageAspect">
        <rdfs:label>logical language aspect</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ontoiop-trm;LanguageAspect"/>
        <owl:disjointWith rdf:resource="&ontoiop-trm;StructuringLanguageAspect"/>
        <skos:definition rdf:datatype="&xsd;string">\termdefinition{logical language aspect}{the (unique) \termref{language aspect} of an \termref{OMS language} that enables the expression of \termref{non-logical symbols} and \termref{sentences} in a logical language}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/LogicalOMSMapping -->

    <owl:Class rdf:about="&ontoiop-trm;LogicalOMSMapping">
        <rdfs:label>logical OMS mapping</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ontoiop-trm;OMSMapping"/>
        <skos:definition rdf:datatype="&xsd;string">  \termdefinition{logical OMS mapping}{\termref{OMS mapping} that has a formal, logic-based semantics}
  \begin{note}
    Logical OMS mappings are given as sets of correspondences, which are required to be \emph{signature morphisms}.
  \end{note}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/Mapping -->

    <owl:Class rdf:about="&ontoiop-trm;Mapping">
        <rdfs:label>mapping</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">morphism (from Category Theory).</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/Model -->

    <owl:Class rdf:about="&ontoiop-trm;Model">
        <rdfs:label>semantic interpretation</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ontoiop-trm;isModelOfSignature"/>
                <owl:onClass rdf:resource="&ontoiop-trm;Vocabulary"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string"> \termdefinition{model}{semantic interpretation of
      all \termref{non-logical symbols} of a \termref{signature}} \begin{note} A model of an OMS is
      a model of the signature of the OMS that moreover \termref{satisfies} all the \termref{axioms}
      of the OMS. \end{note} \begin{note} This term is not to be confused with model in the sense of
      modeling (i.e., the ``M&apos;&apos; in OMS). \end{note} </skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/NonLogicalSymbol -->

    <owl:Class rdf:about="&ontoiop-trm;NonLogicalSymbol">
        <rdfs:label>non-logical symbol</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ontoiop-trm;Term"/>
        <skos:definition rdf:datatype="&xsd;string"> \termdefinition{non-logical symbol\synonym OMS
      symbol}{atomic expression or syntactic constituent of an \termref{OMS} that requires an
      interpretation through a \termref{model}} {\color{red} \begin{note} The notion of ``atomic
      sentence&apos;&apos; used in logic is different, it usually may involve several non-logical
      symbols. \end{note} } \begin{example} Non-logical symbols in OWL \nisref{W3C/TR
      REC-owl2-syntax:2009} (there called ``entities&apos;&apos;) comprise \begin{itemize} \item
      individuals (denoting objects from the domain of discourse), \item classes (denoting sets of
      objects; also called concepts), and \item properties (denoting binary relations over objects;
      also called roles). \end{itemize} This is opposed to logical symbols in OWL, e.g.\ those for
      intersection and union of classes. \end{example} \begin{example} Non-logical symbols in Common
      Logic \nisref{ISO/IEC 24707:2007} comprise \begin{itemize} \item names (denoting objects from
      the domain of discourse), \item sequence markers (denoting sequences of objects).
      \end{itemize} This is opposed to logical symbols in Common Logic, e.g.\ logical connectives
      and quantifiers. \end{example} </skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/OMS -->

    <owl:Class rdf:about="&ontoiop-trm;OMS">
        <rdfs:label>OMS (ontology, specification or model)</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ontoiop-trm;usesLanguage"/>
                <owl:allValuesFrom rdf:resource="&ontoiop-trm;OMSLanguage"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ontoiop-trm;usesLanguage"/>
                <owl:someValuesFrom rdf:resource="&ontoiop-trm;OMSLanguage"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">{OMS (ontology, specification or model)}{set of
      expressions (like \termref{non-logical symbols}, \termref{sentences} and \termref{structuring}
      elements) in a given \termref{OMS language} (or several such languages)}% \begin{note} An
      \termref{OMS} can be written in different \termref{OMS language} \termref{serializations}.
      \end{note} </skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/OMSDocumentation -->

    <owl:Class rdf:about="&ontoiop-trm;OMSDocumentation">
        <rdfs:label>OMS documentation</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ontoiop-trm;SetOfAnnotationExpressions"/>
        <skos:definition rdf:datatype="&xsd;string">  \termdefinition{OMS documentation}{set of all \termref{annotations} to an \termref{OMS}, plus any other documents and explanatory comments generated during the entire OMS building process}
  \begin{note}
    Adapted from \cite{SuarezFigueroaEtAl:OntologyGlossary2008}
  \end{note}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/OMSGraph -->

    <owl:Class rdf:about="&ontoiop-trm;OMSGraph">
        <rdfs:label>OMS graph</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">\termdefinition{OMS graph} graph with \termref{OMSs} as nodes and \termref{OMS mappings} as edges, showing how the OMS are interlinked
  \ednote{add note with examples of what can be these links}
  \begin{note}
    An OMS graph is a diagram of OMS in the sense of category theory, but different from a diagram
    in the sense of model-driven architecture.
  \end{note}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/OMSLanguage -->

    <owl:Class rdf:about="&ontoiop-trm;OMSLanguage">
        <rdfs:label>OMS language</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string"> \termdefinition{OMS language}{language equipped
      with a formal, declarative, logic-based semantics, plus non-logical \termref{annotations}}
      \begin{note} An OMS language is used for the formal specification of \termref{OMSs}.
      \end{note} \begin{example} OMS languages include OWL, Common Logic, F-logic, UML class
      diagrams, RDFS, and OBO. \end{example}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/OMSLanguageTranslation -->

    <owl:Class rdf:about="&ontoiop-trm;OMSLanguageTranslation">
        <rdfs:label>OMS language translation</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ontoiop-trm;LanguageMapping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ontoiop-trm;mappingDomain"/>
                <owl:onClass rdf:resource="&ontoiop-trm;OMSLanguage"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ontoiop-trm;mappingCodomain"/>
                <owl:onClass rdf:resource="&ontoiop-trm;OMSLanguage"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">\termdefinition{OMS language translation}{mapping from constructs in the source \termref{OMS language} to their equivalents in the target \termref{OMS language}}
\begin{note}
  An OMS language translation shall satisfy the property that the
  result of a translation is a well-formed text in the target
  language.
\end{note}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/OMSMapping -->

    <owl:Class rdf:about="&ontoiop-trm;OMSMapping">
        <rdfs:label>OMS mapping</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ontoiop-trm;Mapping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ontoiop-trm;mappingCodomain"/>
                <owl:onClass rdf:resource="&ontoiop-trm;OMS"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ontoiop-trm;mappingDomain"/>
                <owl:onClass rdf:resource="&ontoiop-trm;OMS"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition rdf:datatype="&xsd;string">{relationship between two \termref{OMSs}, typically given as a set of \termref{correspondences}}</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:Class>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/OMSSerialization -->

    <owl:Class rdf:about="&ontoiop-trm;OMSSerialization">
        <rdfs:label>OMS serialization</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ontoiop-trm;serializationOf"/>
                <owl:someValuesFrom rdf:resource="&ontoiop-trm;OMSLanguage"/>
            </owl:Restriction>
        </rdfs:subClassOf>
