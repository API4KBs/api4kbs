     -->

    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/DOL -->

    <owl:NamedIndividual rdf:about="&ontoiop-trm;DOL">
        <rdf:type rdf:resource="&ontoiop-trm;OMSLanguage"/>
        <rdfs:label>DOL</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">\termdefinition{distributed ontology, modeling and
      specification language\synonym DOL}{language for formalizing \termref{distributed OMSs}, whose
      syntax and semantics are specified in this \IS} \begin{note} When viewed as an \termref{OMS
      language}, DOL has \termref{OMSs} as its \termref{non-logical symbols}, and \termref{OMS
      mappings} as its \termref{sentences}. \end{note} </skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.omg.org/spec/OntoIOp/Terminology"/>
    </owl:NamedIndividual>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/Expression -->

    <owl:NamedIndividual rdf:about="&ontoiop-trm;Expression"/>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/Mapping -->

    <owl:NamedIndividual rdf:about="&ontoiop-trm;Mapping"/>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/OMS -->

    <owl:NamedIndividual rdf:about="&ontoiop-trm;OMS"/>
    


    <!-- http://www.omg.org/spec/OntoIOp/Terminology/SetOfExpressions -->

    <owl:NamedIndividual rdf:about="&ontoiop-trm;SetOfExpressions"/>
    


