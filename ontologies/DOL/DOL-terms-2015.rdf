            <rdf:Description rdf:about="&dol;OMS"/>
            <rdf:Description rdf:about="&dol;OMSLanguage"/>
            <rdf:Description rdf:about="&dol;OMSMapping"/>
            <rdf:Description rdf:about="&dol;Serialization"/>
        </owl:members>
    </rdf:Description>
