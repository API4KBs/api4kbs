<!DOCTYPE rdf:RDF [
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY dct "http://purl.org/dc/terms/" >
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#" >
    <!ENTITY sm "http://www.omg.org/techprocess/ab/SpecificationMetadata/" >
    <!ENTITY ontoiop-trm "http://www.omg.org/spec/OntoIOp/OntoIOpTerminology/" >
    <!ENTITY apiforkb-trm "http://www.omg.org/spec/API4KB/API4KBTerminology/" >
]>

<rdf:RDF      xml:base="http://www.omg.org/spec/API4KB/API4KBTerminology/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
     xmlns:ontoiop-trm="http://www.omg.org/spec/OntoIOp/OntoIOpTerminology/"
     xmlns:apiforkb-trm="http://www.omg.org/spec/API4KB/API4KBTerminology/">


    <owl:Ontology rdf:about="http://www.omg.org/spec/API4KB/API4KBTerminology/">
        <rdfs:label>API4KB Terminology Ontology</rdfs:label>


    <!-- Curation and Rights Metadata for the API4KB Terminology Ontology -->

        <sm:copyright rdf:datatype="&xsd;string">Copyright (c) 2014 Athan Services
Copyright (c) 2014 Thematix Partners LLC
Copyright (c) 2014 Object Management Group, Inc.</sm:copyright>
        <dct:license rdf:datatype="&xsd;anyURI">http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense</dct:license>


    <!-- Ontology/File-Level Metadata for the API4KB Terminology Ontology -->

        <sm:filename rdf:datatype="&xsd;string">API4KBOperations.rdf</sm:filename>
        <sm:fileAbbreviation rdf:datatype="&xsd;string">api4kb-trm</sm:fileAbbreviation>
        <owl:versionIRI rdf:resource="http://www.omg.org/spec/API4KB/20150201/API4KBTerminology/"/>
        <sm:fileAbstract rdf:datatype="&xsd;string">This ontology covers terminology used for describing the operations defined by the API4KB specification.</sm:fileAbstract>

        <sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.w3.org/standards/techs/owl#w3c_all</sm:contentLanguage>
        <sm:contentLanguage rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/ODM/</sm:contentLanguage>

        <owl:imports rdf:resource="http://www.omg.org/techprocess/ab/SpecificationMetadata/"/>
      
    </owl:Ontology>
 
  
  <!-- Operations -->
  
  <owl:Class rdf:about="&apiforkb-trm;Operation">
    <rdfs:label>operation</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">function (possibly with side-effects) having inputs and outputs that are API4KB or OntoIOp concepts.</skos:definition>
    <skos:note rdf:datatype="&xsd;string">Option 1. the definition as &quot;function&quot; means that its a black-box standard. We may define the post-conditions by a reference decomposition into actions, but there is no normative requirement for applications to execute according to that decomposition.
      
      Option 2. at the other extreme, the decomposition could also be consider normative.
      
      Option 3. something in between.</skos:note>
    <skos:scopeNote rdf:datatype="&xsd;string">Each API4KB Interface is a subclass of Operation.</skos:scopeNote>
    <skos:scopeNote rdf:datatype="&xsd;string">This class contains only API4KB Operations.</skos:scopeNote>
    <skos:editorialNote rdf:datatype="&xsd;string">is there a super-class for Operation in some OMG standard?</skos:editorialNote>
    <rdfs:subClassOf>
          <owl:Restriction>
            <owl:onProperty rdf:resource="&apiforkb-trm;hasEvent"/>
            <owl:allValuesFrom rdf:resource="&apiforkb-trm;OperationEvent"/>
          </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointUnionOf rdf:parseType="Collection">
      <rdf:Description rdf:about="&apiforkb-trm;SideEffectFreeOperation"/>
      <rdf:Description rdf:about="&apiforkb-trm;SideEffectfulOperation"/>
    </owl:disjointUnionOf>
    <owl:disjointUnionOf rdf:parseType="Collection">
      <rdf:Description rdf:about="&apiforkb-trm;InternalOperation"/>
      <rdf:Description rdf:about="&apiforkb-trm;PublicOperation"/>
    </owl:disjointUnionOf>
  </owl:Class>

  <owl:Class rdf:about="&apiforkb-trm;SideEffectFreeOperation">
    <rdfs:label>side-effect-free operation</rdfs:label>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;Operation"/>
  </owl:Class>
  
  <owl:Class rdf:about="&apiforkb-trm;SideEffectfulOperation">
    <rdfs:label>side-effectful operation</rdfs:label>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;Operation"/>
  </owl:Class>
  <owl:Class rdf:about="&apiforkb-trm;UnaryOperation">
    <rdfs:label>unary function</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">unary function</skos:definition>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="&apiforkb-trm;Operation"/>
          <owl:Restriction>
            <owl:onProperty rdf:resource="&apiforkb-trm;hasEvent"/>
            <owl:someValuesFrom>
              <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                  <rdf:Description rdf:about="&apiforkb-trm;OperationEvent"/>
                  <owl:Restriction>
                    <owl:onProperty rdf:resource="&apiforkb-trm;hasInput"/>
                    <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
                  </owl:Restriction>
                  <owl:Restriction>
                    <owl:onProperty rdf:resource="&apiforkb-trm;hasOutput"/>
                    <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
                  </owl:Restriction>
                </owl:intersectionOf>
              </owl:Class>
            </owl:someValuesFrom>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty rdf:resource="&apiforkb-trm;hasEvent"/>
            <owl:allValuesFrom>
              <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                  <rdf:Description rdf:about="&apiforkb-trm;OperationEvent"/>
                  <owl:Restriction>
                    <owl:onProperty rdf:resource="&apiforkb-trm;hasInput"/>
                    <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
                  </owl:Restriction>
                  <owl:Restriction>
                    <owl:onProperty rdf:resource="&apiforkb-trm;hasOutput"/>
                    <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
                  </owl:Restriction>
                </owl:intersectionOf>
              </owl:Class>
            </owl:allValuesFrom>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class> 
  <!-- Internal Operations -->
  
  <owl:Class rdf:about="&apiforkb-trm;InternalOperation">
    <rdfs:label>internal operation</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">operation offered by the knowledge platform to the proxy.</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;Operation"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;hasEvent"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-trm;InternalOperationEvent"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith
      rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/PublicOperation"/>
  </owl:Class>
  
  <!-- Actions -->

  <owl:Class rdf:about="&apiforkb-trm;Action">
    <rdfs:label>action</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">an atomic operation</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;InternalOperation"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;hasEvent"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-trm;ActionEvent"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <!-- Public Operations -->
  
  <owl:Class rdf:about="&apiforkb-trm;PublicOperation">
    <rdfs:label>public operation</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">operation that is exposed by the proxy to the client.</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;Operation"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;hasEvent"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-trm;PublicOperationEvent"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
   

  <!-- Script Input -->
  
  <owl:Class rdf:about="http://www.omg.org/spec/API4KB/API4KBTerminology/QueryingAction">
    <rdfs:label>querying action</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">action that maps queries and focused OMS resource to collections of answer
      substitutions.</skos:definition>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/Action"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/mapsFromFirst"/>
        <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/QueryResource"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/mapsFromSecond"/>
        <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/FocusedResource"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/mapsTo"/>
        <owl:someValuesFrom rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/AnswerSubstitutionCollection"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  
  <!-- Lowering Actions -->
  
  <owl:Class rdf:about="&apiforkb-trm;LoweringAction">
    <rdfs:label>lowering action</rdfs:label>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;Action"/>
    <skos:definition rdf:datatype="&xsd;string">action that maps a knowledge resource to another knowledge resource of the same &quot;type&quot; (i.e. OMS, Query, Result), belongs to the next lower level of abstraction in the knowledge resource hierarchy, and has the same information content.</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;Action"/>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;UnaryOperation"/>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;SideEffectFreeOperation"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;hasEvent"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-trm;LoweringEvent"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/mapsFrom"/>
        <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/KnowledgeResource"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/mapsTo"/>
        <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/KnowledgeResource"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointUnionOf rdf:parseType="Collection">
      <rdf:Description rdf:about="&apiforkb-trm;OutStreamingAction"/>
      <rdf:Description rdf:about="&apiforkb-trm;MarshallingAction"/>
      <rdf:Description rdf:about="&apiforkb-trm;ManifestingAction"/>
      <rdf:Description rdf:about="&apiforkb-trm;ExpressingAction"/>
    </owl:disjointUnionOf>
  </owl:Class>
  
  <owl:Class rdf:about="&apiforkb-trm;ExpressingAction">
    <rdfs:label>expressing action</rdfs:label>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;LoweringAction"/>
  </owl:Class>
  
  
  
  <owl:Class rdf:about="&apiforkb-trm;ManifestingAction">
    <rdfs:label>manifesting action</rdfs:label>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;LoweringAction"/>
  </owl:Class>
  
  <owl:Class rdf:about="&apiforkb-trm;MarshallingAction">
    <rdfs:label>marshalling action</rdfs:label>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;LoweringAction"/>
  </owl:Class>
  
  <owl:Class rdf:about="&apiforkb-trm;OutStreamingAction">
    <rdfs:label>outstreaming action</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">lifting action that maps knowledge items into knowledge encodings.</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;LoweringAction"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;hasEvent"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-trm;OutStreamingEvent"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/mapsFrom"/>
        <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/KnowledgeEncoding"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/mapsTo"/>
        <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/KnowledgeItem"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <!-- Lifting Events -->
  
  <owl:Class rdf:about="&apiforkb-trm;LiftingAction">
    <rdfs:label>lifting action</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">action whose input is a knowledge resource and whose output is a knowledge resource of the same &quot;type&quot; (i.e. OMS, Query, Result), belongs to the next higher level of abstraction in the knowledge resource hierarchy and has the same information content.</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;Action"/>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;UnaryOperation"/>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;SideEffectFreeOperation"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;hasEvent"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-trm;LiftingEvent"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/mapsFrom"/>
        <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/KnowledgeResource"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/mapsTo"/>
        <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/KnowledgeResource"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointUnionOf rdf:parseType="Collection">
      <rdf:Description rdf:about="&apiforkb-trm;InStreamingAction"/>
      <rdf:Description rdf:about="&apiforkb-trm;UnmarshallingAction"/>
      <rdf:Description rdf:about="&apiforkb-trm;ParsingAction"/>
      <rdf:Description rdf:about="&apiforkb-trm;ConceptualizingAction"/>
    </owl:disjointUnionOf>
  </owl:Class>

  <owl:Class rdf:about="&apiforkb-trm;InStreamingAction">
    <rdfs:label>instreaming action</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">lifting action that maps knowledge items into knowledge encodings.</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;LiftingAction"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;hasEvent"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-trm;InStreamingEvent"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/mapsFrom"/>
        <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/KnowledgeItem"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/mapsTo"/>
        <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/KnowledgeEncoding"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
  
  <owl:Class rdf:about="http://www.omg.org/spec/API4KB/API4KBTerminology/UnmarshallingAction">
    <rdfs:label>unmarshalling action</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">lifting action that maps knowledge encodings into knowledge manifestations.</skos:definition>
    <rdfs:subClassOf rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/LiftingAction"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;hasEvent"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-trm;UnmarshallingEvent"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/mapsFrom"/>
        <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/KnowledgeEncoding"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/mapsTo"/>
        <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/KnowledgeManifestation"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
  <owl:Class rdf:about="&apiforkb-trm;ParsingAction">
    <rdfs:label>parsing action</rdfs:label>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;LiftingAction"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;hasEvent"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-trm;ParsingEvent"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;mapsFrom"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-trm;KnowledgeManifestation"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;mapsTo"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-trm;GeneralizedExpression"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
  <owl:Class rdf:about="&apiforkb-trm;ConceptualizingAction">
    <rdfs:label>conceptualizing action</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">lifting action that transforms generalized expressions to knowledge assets.</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;LiftingAction"/>
    <rdfs:subClassOf>
          <owl:Restriction>
            <owl:onProperty rdf:resource="&apiforkb-trm;hasEvent"/>
            <owl:allValuesFrom rdf:resource="&apiforkb-trm;ConceptualizingEvent"/>
          </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;mapsFrom"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-trm;GeneralizedExpression"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;mapsTo"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-trm;KnowledgeAsset"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
  
  
  
  <!-- Public Operations -->
  
  <owl:Class rdf:about="&apiforkb-trm;AddingOperation-KnowledgeResourceToImmutableKB">
    <rdfs:label>operation adding knowledge resources to immutable KBs</rdfs:label>
    <skos:definition rdf:datatype="&xsd;string">public operation adding knowlede
      resources to immutable knowledge bases</skos:definition>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;PublicOperation"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/mapsFromFirst"/>
        <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/KnowledgeResource"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/mapsFromSecond"/>
        <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/ImmutableKnowledgeBase"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/mapsTo"/>
        <owl:allValuesFrom rdf:resource="http://www.omg.org/spec/API4KB/API4KBTerminology/ImmutableKnowledgeBase"/>
      </owl:Restriction>
    </rdfs:subClassOf>    
  </owl:Class>  
  
  <owl:Class rdf:about="&apiforkb-trm;AddingOperation-QueryItemToImmutableKB">
    <rdfs:label>operation adding knowledge resources that are query items to immutable KBs</rdfs:label>
    <rdfs:subClassOf rdf:resource="&apiforkb-trm;AddingOperation-KnowledgeResourceToImmutableKB"/>
