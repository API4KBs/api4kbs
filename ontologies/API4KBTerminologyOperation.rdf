    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;mapsFrom"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-trm;KnowledgeManifestation"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&apiforkb-trm;mapsTo"/>
        <owl:allValuesFrom rdf:resource="&apiforkb-trm;KnowledgeEncoding"/>
      </owl:Restriction>
    </rdfs:subClassOf>
