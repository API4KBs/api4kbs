AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#definition> :specificationURL "URL for the specification directory on the OMG web site"^^xsd:string)
AnnotationPropertyRange(:specificationURL xsd:anyURI)
AnnotationPropertyDomain(:specificationURL :Specification)

# Annotation Property: :specificationVersionStatus (specification version status)

AnnotationAssertion(rdfs:label :specificationVersionStatus "specification version status")
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#definition> :specificationVersionStatus "Status of this version of the specification from a process perspective, including whether or not it has been formally adopted or superseded by a newer version."^^xsd:string)
SubAnnotationPropertyOf(:specificationVersionStatus <http://www.w3.org/2004/02/skos/core#note>)
AnnotationPropertyRange(:specificationVersionStatus xsd:string)
AnnotationPropertyDomain(:specificationVersionStatus :Specification)

