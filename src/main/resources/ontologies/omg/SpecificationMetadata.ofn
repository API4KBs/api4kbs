AnnotationAssertion(rdfs:label :specificationVersionURL "specification version URL")
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#definition> :specificationVersionURL "Explicit URI where the specification version is published"^^xsd:string)
AnnotationPropertyRange(:specificationVersionURL xsd:anyURI)
AnnotationPropertyDomain(:specificationVersionURL :Specification)

# Annotation Property: :submitter (submitter)

AnnotationAssertion(rdfs:label :submitter "submitter")
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#definition> :submitter "a contributor that is an OMG member, identified member of the submission team, and filed the requisite LOI, responsible for the content of the specification or model"^^xsd:string)
SubAnnotationPropertyOf(:submitter :contributor)
AnnotationPropertyDomain(:submitter :Specification)

